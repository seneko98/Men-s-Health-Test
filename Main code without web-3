                   "‚Ä¢ –ô–æ–≥–∞, –ø–ª–∞–≤–∞–Ω–Ω—è, –ø—ñ–ª–∞—Ç–µ—Å - –ø–æ–∫—Ä–∞—â—É—é—Ç—å –∫—Ä–æ–≤–æ–æ–±—ñ–≥ –≤ –º–∞–ª–æ–º—É —Ç–∞–∑—É\n"
                   "‚Ä¢ –ü—Ä–æ–≥—É–ª—è–Ω–∫–∏ –Ω–∞ —Å–≤—ñ–∂–æ–º—É –ø–æ–≤—ñ—Ç—Ä—ñ\n"
                   "‚Ä¢ –ê–¥–∞–ø—Ç–∞—Ü—ñ—è —Ç—Ä–µ–Ω—É–≤–∞–Ω—å –ø—ñ–¥ —Ñ–∞–∑–∏ —Ü–∏–∫–ª—É\n\n"
                   "4Ô∏è‚É£ *–•–∞—Ä—á—É–≤–∞–Ω–Ω—è:*\n"
                   "‚Ä¢ –ö–æ–Ω—Ç—Ä–æ–ª—å —Ä—ñ–≤–Ω—è —Ü—É–∫—Ä—É –≤ –∫—Ä–æ–≤—ñ\n"
                   "‚Ä¢ –†–æ–∑–ø–æ–¥—ñ–ª –≤—É–≥–ª–µ–≤–æ–¥—ñ–≤ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω–æ –¥–æ —Ñ–∞–∑ —Ü–∏–∫–ª—É\n"
                   "‚Ä¢ –î–æ—Å—Ç–∞—Ç–Ω—è –∫—ñ–ª—å–∫—ñ—Å—Ç—å –±—ñ–ª–∫—É (1-1.2 –≥/–∫–≥ –º–∞—Å–∏ —Ç—ñ–ª–∞)\n\n"
                   "5Ô∏è‚É£ *–ü—Ä–∏ –ü–ú–°:*\n"
                   "‚Ä¢ –í—ñ—Ç–∞–º—ñ–Ω B6 - 50-100 –º–≥/–¥–µ–Ω—å\n"
                   "‚Ä¢ –ú–∞–≥–Ω—ñ–π - 300-400 –º–≥/–¥–µ–Ω—å\n"
                   "‚Ä¢ –ú–∞—Å–ª–æ –ø—Ä–∏–º—É–ª–∏ –≤–µ—á—ñ—Ä–Ω—å–æ—ó\n\n"
                   "–î–ª—è —ñ–Ω–¥–∏–≤—ñ–¥—É–∞–ª—å–Ω–æ—ó —ñ–Ω—Ç–µ—Ä–ø—Ä–µ—Ç–∞—Ü—ñ—ó —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ —Ç–∞ —Ä–æ–∑—Ä–æ–±–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø–ª–∞–Ω—É, "
                   "–±—É–¥—å –ª–∞—Å–∫–∞, –∑–≤–µ—Ä–Ω—ñ—Ç—å—Å—è –∑–∞ –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—î—é –¥–æ —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–∞: https://t.me/vladyslav_kravchukk"
               )

           await query.message.reply_text(extended_text, parse_mode='Markdown')
           return ConversationHandler.END

       elif query.data == "training_program":
           # –ü—Ä–æ–ø–æ–∑–∏—Ü—ñ—è –ø—Ä–æ–≥—Ä–∞–º–∏ —Ç—Ä–µ–Ω—É–≤–∞–Ω—å
           if age_group == "–î–æ 25 —Ä–æ–∫—ñ–≤":
               program_title = "–ü–†–û–ì–†–ê–ú–ê –¢–†–ï–ù–£–í–ê–ù–¨ –î–õ–Ø –ú–ê–ö–°–ò–ú–ê–õ–¨–ù–û–ì–û –ù–ê–ë–û–†–£ –ú'–Ø–ó–û–í–û–á –ú–ê–°–ò"
               program_price = "599 –≥—Ä–Ω"
           elif age_group == "25-35 —Ä–æ–∫—ñ–≤":
               program_title = "–ü–†–û–ì–†–ê–ú–ê –¢–†–ï–ù–£–í–ê–ù–¨ –ó –£–†–ê–•–£–í–ê–ù–ù–Ø–ú –ì–û–†–ú–û–ù–ê–õ–¨–ù–ò–• –û–°–û–ë–õ–ò–í–û–°–¢–ï–ô"
               program_price = "699 –≥—Ä–Ω"
           elif age_group in ["36-45 —Ä–æ–∫—ñ–≤", "46-60 —Ä–æ–∫—ñ–≤"]:
               program_title = "–°–ò–°–¢–ï–ú–ê –¢–†–ï–ù–£–í–ê–ù–¨ –î–õ–Ø –ó–ë–ï–†–ï–ñ–ï–ù–ù–Ø –ú'–Ø–ó–û–í–û–á –ú–ê–°–ò 35+"
               program_price = "799 –≥—Ä–Ω"
           else:  # 60+
               program_title = "–ê–î–ê–ü–¢–û–í–ê–ù–Ü –§–Ü–ó–ò–ß–ù–Ü –ù–ê–í–ê–ù–¢–ê–ñ–ï–ù–ù–Ø –î–õ–Ø –ü–Ü–î–¢–†–ò–ú–ö–ò –¢–ï–°–¢–û–°–¢–ï–†–û–ù–£ 60+"
               program_price = "699 –≥—Ä–Ω"

           product_text = (
               f"*{program_title}*\n\n"
               "–ù–∞—à–∞ –ø—Ä–æ–≥—Ä–∞–º–∞ —Ä–æ–∑—Ä–æ–±–ª–µ–Ω–∞ –∑ —É—Ä–∞—Ö—É–≤–∞–Ω–Ω—è–º –≤–∞—à–æ–≥–æ –≤—ñ–∫—É —Ç–∞ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é.\n\n"
               "‚úÖ –ü–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω–∏–π –ø–ª–∞–Ω —Ç—Ä–µ–Ω—É–≤–∞–Ω—å –Ω–∞ 8 —Ç–∏–∂–Ω—ñ–≤\n"
               "‚úÖ –í—ñ–¥–µ–æ—ñ–Ω—Å—Ç—Ä—É–∫—Ü—ñ—ó –∑ –ø—Ä–∞–≤–∏–ª—å–Ω–æ—é —Ç–µ—Ö–Ω—ñ–∫–æ—é –≤–∏–∫–æ–Ω–∞–Ω–Ω—è\n"
               "‚úÖ –û–ø—Ç–∏–º–∞–ª—å–Ω–µ –Ω–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è –¥–ª—è —Å—Ç–∏–º—É–ª—è—Ü—ñ—ó –≤–∏—Ä–æ–±–ª–µ–Ω–Ω—è —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É\n"
               "‚úÖ –©–æ—Ç–∏–∂–Ω–µ–≤–∞ –∫–æ—Ä–µ–∫—Ü—ñ—è –ø—Ä–æ–≥—Ä–∞–º–∏ –¥–ª—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ—ó –µ—Ñ–µ–∫—Ç–∏–≤–Ω–æ—Å—Ç—ñ\n"
               "‚úÖ –ü—ñ–¥—Ç—Ä–∏–º–∫–∞ —Ç—Ä–µ–Ω–µ—Ä–∞ –ø—Ä–æ—Ç—è–≥–æ–º –≤—Å—å–æ–≥–æ –∫—É—Ä—Å—É\n\n"
               f"üí∞ –í–∞—Ä—Ç—ñ—Å—Ç—å –ø—Ä–æ–≥—Ä–∞–º–∏: {program_price}\n\n"
               "–î–ª—è –æ—Ç—Ä–∏–º–∞–Ω–Ω—è –ø—Ä–æ–≥—Ä–∞–º–∏ –∑–≤'—è–∂—ñ—Ç—å—Å—è –∑ –Ω–∞—à–∏–º —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–æ–º:\n"
               "üë®‚Äç‚öïÔ∏è [–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—è —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–∞](https://t.me/vladyslav_kravchukk)"
           )

           await query.message.reply_text(product_text, parse_mode='Markdown')
           return ConversationHandler.END

       elif query.data == "libido_guide":
           # –ü—Ä–æ–ø–æ–∑–∏—Ü—ñ—è –≥–∞–π–¥—É –Ω–∞ –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è –ª—ñ–±—ñ–¥–æ
           if age_group == "–î–æ 25 —Ä–æ–∫—ñ–≤":
               guide_title = "–ì–ê–ô–î –ù–ê –ü–û–°–ò–õ–ï–ù–ù–Ø –õ–Ü–ë–Ü–î–û –î–õ–Ø –ú–û–õ–û–î–ò–• –ß–û–õ–û–í–Ü–ö–Ü–í"
               guide_price = "399 –≥—Ä–Ω"
               guide_focus = "–º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ—ó –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–æ—Å—Ç—ñ —Ç–∞ –≤–∏—Ç—Ä–∏–≤–∞–ª–æ—Å—Ç—ñ"
           elif age_group == "25-35 —Ä–æ–∫—ñ–≤":
               guide_title = "–ì–ê–ô–î –ù–ê –ü–û–°–ò–õ–ï–ù–ù–Ø –õ–Ü–ë–Ü–î–û –î–õ–Ø –ê–ö–¢–ò–í–ù–ò–• –ß–û–õ–û–í–Ü–ö–Ü–í"
               guide_price = "499 –≥—Ä–Ω"
               guide_focus = "—Å—Ç–∞–±—ñ–ª—å–Ω–æ—Å—Ç—ñ —Ç–∞ –µ–Ω–µ—Ä–≥—ñ–π–Ω–æ—Å—Ç—ñ"
           elif age_group in ["36-45 —Ä–æ–∫—ñ–≤", "46-60 —Ä–æ–∫—ñ–≤"]:
               guide_title = "–ì–ê–ô–î –ó –í–Ü–î–ù–û–í–õ–ï–ù–ù–Ø –°–ï–ö–°–£–ê–õ–¨–ù–û–á –§–£–ù–ö–¶–Ü–á 35+"
               guide_price = "599 –≥—Ä–Ω"
               guide_focus = "–≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è –ø—Ä–∏—Ä–æ–¥–Ω–æ–≥–æ –ø–æ—Ç–µ–Ω—Ü—ñ–∞–ª—É"
           else:  # 60+
               guide_title = "–ö–û–ú–ü–õ–ï–ö–°–ù–ò–ô –ì–ê–ô–î –ó –ü–û–ö–†–ê–©–ï–ù–ù–Ø –°–ï–ö–°–£–ê–õ–¨–ù–û–ì–û –ó–î–û–†–û–í'–Ø 60+"
               guide_price = "699 –≥—Ä–Ω"
               guide_focus = "—è–∫—ñ—Å–Ω–æ–≥–æ –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è —Ç–∞ –ø—ñ–¥—Ç—Ä–∏–º–∫–∏ —Å–µ–∫—Å—É–∞–ª—å–Ω–æ—ó —Ñ—É–Ω–∫—Ü—ñ—ó"

           product_text = (
               f"*{guide_title}*\n\n"
               f"–ï–∫—Å–∫–ª—é–∑–∏–≤–Ω–∏–π –≥–∞–π–¥ –¥–ª—è {guide_focus}:\n\n"
               "‚úÖ –°–ø–∏—Å–æ–∫ –Ω–µ–æ–±—Ö—ñ–¥–Ω–∏—Ö –¥–æ–±–∞–≤–æ–∫ –¥–ª—è —Å—Ç–∏–º—É–ª—è—Ü—ñ—ó –ª—ñ–±—ñ–¥–æ\n"
               "‚úÖ –†–µ–∂–∏–º –ø—Ä–∏–π–æ–º—É –¥–æ–±–∞–≤–æ–∫: –≤ –¥–µ–Ω—å —Å–µ–∫—Å—É, –ø–µ—Ä–µ–¥ —ñ –ø—ñ—Å–ª—è\n"
               "‚úÖ –ü—Ä–æ–¥—É–∫—Ç–∏, —è–∫—ñ –ø—Ä–∏—Ä–æ–¥–Ω–æ –ø—ñ–¥–≤–∏—â—É—é—Ç—å —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω\n"
               "‚úÖ –¢—Ä–∞–≤'—è–Ω—ñ —Ç–∞ –∞–ø—Ç–µ—á–Ω—ñ –∑–∞—Å–æ–±–∏ –¥–ª—è –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Å–µ–∫—Å—É–∞–ª—å–Ω–æ—ó —Ñ—É–Ω–∫—Ü—ñ—ó\n"
               "‚úÖ –í–ø—Ä–∞–≤–∏ –¥–ª—è —Å—Ç–∏–º—É–ª—è—Ü—ñ—ó –∫—Ä–æ–≤–æ–æ–±—ñ–≥—É –≤ –º–∞–ª–æ–º—É —Ç–∞–∑—É\n"
               "‚úÖ –¢–µ—Ö–Ω—ñ–∫–∏ —É–ø—Ä–∞–≤–ª—ñ–Ω–Ω—è –µ–Ω–µ—Ä–≥—ñ—î—é –¥–ª—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ—ó –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–æ—Å—Ç—ñ\n\n"
               f"üí∞ –í–∞—Ä—Ç—ñ—Å—Ç—å –≥–∞–π–¥—É: {guide_price}\n\n"
               "üì± –§–æ—Ä–º–∞—Ç: PDF-—Ñ–∞–π–ª –∑ –¥–µ—Ç–∞–ª—å–Ω–∏–º–∏ —ñ–Ω—Å—Ç—Ä—É–∫—Ü—ñ—è–º–∏\n"
               "üöö –î–æ—Å—Ç–∞–≤–∫–∞: –º–∏—Ç—Ç—î–≤–æ –ø—ñ—Å–ª—è –æ–ø–ª–∞—Ç–∏ –Ω–∞ –≤–∞—à email\n\n"
               "–î–ª—è –æ—Ç—Ä–∏–º–∞–Ω–Ω—è –≥–∞–π–¥—É –∑–≤'—è–∂—ñ—Ç—å—Å—è –∑ –Ω–∞—à–∏–º —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–æ–º:\n"
               "üë®‚Äç‚öïÔ∏è [–ó–∞–º–æ–≤–∏—Ç–∏ –≥–∞–π–¥](https://t.me/vladyslav_kravchukk)"
           )

           await query.message.reply_text(product_text, parse_mode='Markdown')
           return ConversationHandler.END

       elif query.data == "hair_guide":
           # Add handler for hair guide button
           if gender == "–ß–æ–ª–æ–≤—ñ–∫":
               guide_title = "–ì–ê–ô–î –ü–û –ó–ë–ï–†–ï–ñ–ï–ù–ù–Æ –í–û–õ–û–°–°–Ø –¢–ê –í–ò–†–û–©–£–í–ê–ù–ù–Æ –ë–û–†–û–î–ò"
               guide_focus = "–ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Ä–æ—Å—Ç—É —Ç–∞ —è–∫–æ—Å—Ç—ñ –±–æ—Ä–æ–¥–∏"
           else:
               guide_title = "–ì–ê–ô–î –ü–û –ó–î–û–†–û–í'–Æ –¢–ê –ì–£–°–¢–û–¢–Ü –í–û–õ–û–°–°–Ø"  
               guide_focus = "–∑–¥–æ—Ä–æ–≤'—è —Ç–∞ –∫—Ä–∞—Å–∏ –≤–æ–ª–æ—Å—Å—è"

           product_text = (
               f"*{guide_title}*\n\n"
               f"–ï–∫—Å–∫–ª—é–∑–∏–≤–Ω–∏–π –≥–∞–π–¥ –¥–ª—è {guide_focus}:\n\n"
               "‚úÖ –ù–µ–æ–±—Ö—ñ–¥–Ω—ñ –ø—Ä–µ–ø–∞—Ä–∞—Ç–∏ —Ç–∞ –¥–æ–±–∞–≤–∫–∏\n"
               "‚úÖ –ü–æ—Ä–∞–¥–∏ —â–æ–¥–æ –¥–æ–≥–ª—è–¥—É —Ç–∞ —Ä–µ–∂–∏–º—É\n"
               "‚úÖ –ü—Ä–æ–¥—É–∫—Ç–∏ –¥–ª—è –∑–¥–æ—Ä–æ–≤'—è –≤–æ–ª–æ—Å—Å—è\n\n"
               "üí∞ –í–∞—Ä—Ç—ñ—Å—Ç—å –≥–∞–π–¥—É: 399 –≥—Ä–Ω\n\n"
               "–î–ª—è –æ—Ç—Ä–∏–º–∞–Ω–Ω—è –≥–∞–π–¥—É –∑–≤'—è–∂—ñ—Ç—å—Å—è –∑ –Ω–∞—à–∏–º —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–æ–º:\n"
               "üë®‚Äç‚öïÔ∏è [–ó–∞–º–æ–≤–∏—Ç–∏ –≥–∞–π–¥](https://t.me/vladyslav_kravchukk)"
           )

           await query.message.reply_text(product_text, parse_mode='Markdown')
           return ConversationHandler.END

   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ handle_result_buttons: {e}", exc_info=True)
       await query.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ handle_result_buttons: {e}", exc_info=True)
       await query.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

def main():
   try:
       print("–ó–∞–ø—É—Å–∫ –±–æ—Ç–∞...")
       logger.info("–ü–æ—á–∏–Ω–∞—î–º–æ –∑–∞–ø—É—Å–∫ –±–æ—Ç–∞...")

       # –í–∏–¥–∞–ª–µ–Ω–Ω—è webhook –ø–µ—Ä–µ–¥ —Å—Ç–≤–æ—Ä–µ–Ω–Ω—è–º –∑–∞—Å—Ç–æ—Å—É–Ω–∫—É
       import requests
       print("–í–∏–¥–∞–ª—è—î–º–æ webhook...")
       webhook_response = requests.get(f"https://api.telegram.org/bot{TOKEN}/deleteWebhook")
       print(f"–í—ñ–¥–ø–æ–≤—ñ–¥—å –Ω–∞ –≤–∏–¥–∞–ª–µ–Ω–Ω—è webhook: {webhook_response.status_code}, {webhook_response.text}")

       # –°—Ç–≤–æ—Ä–µ–Ω–Ω—è –∑–∞—Å—Ç–æ—Å—É–Ω–∫—É –±–µ–∑ –¥–æ–¥–∞—Ç–∫–æ–≤–∏—Ö –ø–∞—Ä–∞–º–µ—Ç—Ä—ñ–≤
       print("–°—Ç–≤–æ—Ä—é—î–º–æ –∑–∞—Å—Ç–æ—Å—É–Ω–æ–∫ (—Å–ø—Ä–æ—â–µ–Ω–∞ –≤–µ—Ä—Å—ñ—è)...")
       application = ApplicationBuilder().token(TOKEN).build()
       print("–ó–∞—Å—Ç–æ—Å—É–Ω–æ–∫ —Å—Ç–≤–æ—Ä–µ–Ω–æ.")

       # –°—Ç–≤–æ—Ä–µ–Ω–Ω—è –æ–±—Ä–æ–±–Ω–∏–∫–∞ —Ä–æ–∑–º–æ–≤–∏
       print("–°—Ç–≤–æ—Ä—é—î–º–æ –æ–±—Ä–æ–±–Ω–∏–∫ —Ä–æ–∑–º–æ–≤–∏...")
       conv_handler = ConversationHandler(
           entry_points=[CommandHandler("start", start)],
           states={
               NAME: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_name)],
               AGE: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_age)],
               GENDER: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_gender)],
               BODY_TYPE: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_body_type)],
               HEIGHT: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_height)],
               WEIGHT: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_weight)],
               QUESTIONS: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_answer)],
               PROCESSING: [MessageHandler(filters.TEXT & ~filters.COMMAND, process_results)],
               HORMONE_MENU: [MessageHandler(filters.TEXT & ~filters.COMMAND, handle_hormone_menu)],
           },
           fallbacks=[CommandHandler("cancel", cancel)],
       )
       print("–û–±—Ä–æ–±–Ω–∏–∫ —Ä–æ–∑–º–æ–≤–∏ —Å—Ç–≤–æ—Ä–µ–Ω–æ.")

       # –î–æ–¥–∞–≤–∞–Ω–Ω—è –æ–±—Ä–æ–±–Ω–∏–∫—ñ–≤
       print("–î–æ–¥–∞—î–º–æ –æ–±—Ä–æ–±–Ω–∏–∫–∏...")
       application.add_handler(conv_handler)
       print("–î–æ–¥–∞–Ω–æ conv_handler.")
       application.add_handler(CommandHandler("consult", handle_consult))
       print("–î–æ–¥–∞–Ω–æ consult handler.")
       application.add_handler(CallbackQueryHandler(handle_result_buttons))
       print("–î–æ–¥–∞–Ω–æ callback handler.")

       # –î–æ–¥–∞–≤–∞–Ω–Ω—è –æ–±—Ä–æ–±–Ω–∏–∫–∞ –ø–æ–º–∏–ª–æ–∫
       print("–î–æ–¥–∞—î–º–æ –æ–±—Ä–æ–±–Ω–∏–∫ –ø–æ–º–∏–ª–æ–∫...")
       application.add_error_handler(error_handler)
       print("–û–±—Ä–æ–±–Ω–∏–∫ –ø–æ–º–∏–ª–æ–∫ –¥–æ–¥–∞–Ω–æ.")

       # –ü–µ—Ä–µ–≤—ñ—Ä–∫–∞ —Ä–æ–±–æ—Ç–∏ –±–æ—Ç–∞
       print("–ü–µ—Ä–µ–≤—ñ—Ä—è—î–º–æ –±–æ—Ç–∞...")
       test_response = requests.get(f"https://api.telegram.org/bot{TOKEN}/getMe")
       print(f"–°—Ç–∞—Ç—É—Å –≤—ñ–¥–ø–æ–≤—ñ–¥—ñ: {test_response.status_code}")
       if test_response.status_code == 200:
           bot_info = test_response.json()
           if bot_info.get('ok'):
               print(f"–ë–æ—Ç —É—Å–ø—ñ—à–Ω–æ –ø—ñ–¥–∫–ª—é—á–µ–Ω–∏–π: @{bot_info['result'].get('username')}")
               logger.info(f"–ë–æ—Ç —É—Å–ø—ñ—à–Ω–æ –ø—ñ–¥–∫–ª—é—á–µ–Ω–∏–π: @{bot_info['result'].get('username')}")
           else:
               print(f"–ü–æ–º–∏–ª–∫–∞ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏ –±–æ—Ç–∞: {bot_info}")
               logger.error(f"–ü–æ–º–∏–ª–∫–∞ –ø–µ—Ä–µ–≤—ñ—Ä–∫–∏ –±–æ—Ç–∞: {bot_info}")
       else:
           print(f"–ü–æ–º–∏–ª–∫–∞ –∑'—î–¥–Ω–∞–Ω–Ω—è –∑ Telegram API: {test_response.status_code}")
           logger.error(f"–ü–æ–º–∏–ª–∫–∞ –∑'—î–¥–Ω–∞–Ω–Ω—è –∑ Telegram API: {test_response.status_code}")

       # –ó–∞–ø—É—Å–∫ –±–æ—Ç–∞
       print("–ó–∞–ø—É—Å–∫–∞—î–º–æ –±–æ—Ç–∞...")
       logger.info("–ë–æ—Ç –∑–∞–ø—É—â–µ–Ω–æ")
       application.run_polling(allowed_updates=["message", "callback_query"])
       print("–ë–æ—Ç –∑–∞–≤–µ—Ä—à–∏–≤ —Ä–æ–±–æ—Ç—É.")

   except Exception as e:
       print(f"–ö–†–ò–¢–ò–ß–ù–ê –ü–û–ú–ò–õ–ö–ê: {e}")
       logger.error(f"–ö—Ä–∏—Ç–∏—á–Ω–∞ –ø–æ–º–∏–ª–∫–∞ –≤ main(): {e}", exc_info=True)
       import traceback
       traceback.print_exc()

# –¢–æ—á–∫–∞ –≤—Ö–æ–¥—É
if __name__ == "__main__":
   main()
