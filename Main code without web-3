   "–ê–Ω–∞–ª—ñ–∑—É—é —Ç–≤–æ—ó –≤—ñ–¥–ø–æ–≤—ñ–¥—ñ...",
   "–û–±—Ä–æ–±–ª—è—é –ø–æ—á–∞—Ç–∫–æ–≤—ñ –¥–∞–Ω—ñ...",
   "–§–æ—Ä–º—É—é –ø–æ–ø–µ—Ä–µ–¥–Ω—é –æ—Ü—ñ–Ω–∫—É –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä—ñ–≤..."
]

processing_phrases_middle = [
   "–ê–Ω–∞–ª—ñ–∑ –≤–∑–∞—î–º–æ–∑–≤'—è–∑–∫—É –º—ñ–∂ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞–º–∏...",
   "–í–∏–≤—á–∞—é –≤–ø–ª–∏–≤ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ —Ñ–æ–Ω—É –Ω–∞ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∏...",
   "–û—Ü—ñ–Ω–∫–∞ –≤–ø–ª–∏–≤—É –∞–Ω—Ç—Ä–æ–ø–æ–º–µ—Ç—Ä–∏—á–Ω–∏—Ö –¥–∞–Ω–∏—Ö –Ω–∞ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–π –±–∞–ª–∞–Ω—Å...",
   "–ü–æ—Ä—ñ–≤–Ω—è–Ω–Ω—è –ø–æ–∫–∞–∑–Ω–∏–∫—ñ–≤ –∑ —Ä–µ—Ñ–µ—Ä–µ–Ω—Ç–Ω–∏–º–∏ –∑–Ω–∞—á–µ–Ω–Ω—è–º–∏...",
   "–ê–Ω–∞–ª—ñ–∑ —ñ–Ω–¥–∏–≤—ñ–¥—É–∞–ª—å–Ω–∏—Ö –æ—Å–æ–±–ª–∏–≤–æ—Å—Ç–µ–π –ø—Ä–æ—Ñ—ñ–ª—é..."
]

processing_phrases_end = [
   "–í–∏–∑–Ω–∞—á–∞—é –æ–ø—Ç–∏–º–∞–ª—å–Ω—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó...",
   "–§–æ—Ä–º—É—é –ø–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω–∏–π –∑–≤—ñ—Ç...",
   "–û—Ü—ñ–Ω—é—é –∫–æ—Ä–µ–ª—è—Ü—ñ—é –º—ñ–∂ –ø–æ–∫–∞–∑–Ω–∏–∫–∞–º–∏ —Ä—ñ–∑–Ω–∏—Ö —Å–∏—Å—Ç–µ–º...",
   "–ü–æ—Ä—ñ–≤–Ω—é—é —Ç–≤–æ—ó —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏ –∑ –±–∞–∑–æ—é –¥–∞–Ω–∏—Ö...",
   "–í–∏–∑–Ω–∞—á–∞—é –Ω–∞–π–µ—Ñ–µ–∫—Ç–∏–≤–Ω—ñ—à—ñ —Å—Ç—Ä–∞—Ç–µ–≥—ñ—ó –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è –±–∞–ª–∞–Ω—Å—É...",
   "–†–æ–∑—Ä–∞—Ö—É–Ω–æ–∫ —ñ–Ω–¥–∏–≤—ñ–¥—É–∞–ª—å–Ω–∏—Ö —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ–π...",
   "–ó–∞–≤–µ—Ä—à—É—é –∞–Ω–∞–ª—ñ–∑ –Ω–µ–π—Ä–æ–ø—Ä–æ—Ñ—ñ–ª—é..."
]

# –§—É–Ω–∫—Ü—ñ—è –¥–ª—è —Ä–æ–∑—Ä–∞—Ö—É–Ω–∫—É –Ü–ú–¢
def calculate_bmi(weight, height):
   try:
       # –í–∏—Å–æ—Ç–∞ –≤ –º–µ—Ç—Ä–∞—Ö (–ø—Ä–∏—Ö–æ–¥–∏—Ç—å –≤ —Å–º)
       height_m = height / 100
       # –§–æ—Ä–º—É–ª–∞ –Ü–ú–¢: –≤–∞–≥–∞ (–∫–≥) / –∑—Ä—ñ—Å—Ç¬≤ (–º)
       bmi = weight / (height_m * height_m)
       return round(bmi, 1)
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É calculate_bmi: {e}")
       return 0

# –§—É–Ω–∫—Ü—ñ—è –¥–ª—è –æ—Ü—ñ–Ω–∫–∏ –∫–∞—Ç–µ–≥–æ—Ä—ñ—ó –Ü–ú–¢ –∑ —É—Ä–∞—Ö—É–≤–∞–Ω–Ω—è–º —Ç–∏–ø—É —Å—Ç–∞—Ç—É—Ä–∏
def get_bmi_category(bmi, body_type, weight):
   try:
       if body_type == "–°–ø–æ—Ä—Ç–∏–≤–Ω–∏–π":
           if bmi < 18.5:
               return "–ù–µ–¥–æ—Å—Ç–∞—Ç–Ω—è –≤–∞–≥–∞"
           elif bmi < 27:  # –†–æ–∑—à–∏—Ä–µ–Ω–∏–π –¥—ñ–∞–ø–∞–∑–æ–Ω –¥–ª—è —Å–ø–æ—Ä—Ç—Å–º–µ–Ω—ñ–≤
               return "–ù–æ—Ä–º–∞–ª—å–Ω–∞ –≤–∞–≥–∞"
           elif bmi < 30 or weight < 100:
               return "–õ–µ–≥–∫–∏–π –Ω–∞–¥–ª–∏—à–æ–∫ –º–∞—Å–∏ - –º–æ–∂–µ –±—É—Ç–∏ –ø–æ–≤'—è–∑–∞–Ω–∏–π –∑ –º'—è–∑–æ–≤–æ—é –º–∞—Å–æ—é"
           else:
               return "–ù–∞–¥–º—ñ—Ä–Ω–∞ –≤–∞–≥–∞ - —Ä–µ–∫–æ–º–µ–Ω–¥—É—î—Ç—å—Å—è –ø—ñ–¥—Å—É—à–∏—Ç–∏—Å—å"
       else:
           if bmi < 18.5:
               return "–ù–µ–¥–æ—Å—Ç–∞—Ç–Ω—è –≤–∞–≥–∞"
           elif bmi < 25:
               return "–ù–æ—Ä–º–∞–ª—å–Ω–∞ –≤–∞–≥–∞"
           elif bmi < 30:
               return "–ù–∞–¥–º—ñ—Ä–Ω–∞ –≤–∞–≥–∞ - —Ä–µ–∫–æ–º–µ–Ω–¥—É—î—Ç—å—Å—è —Å—Ö—É–¥–Ω–µ–Ω–Ω—è"
           else:
               return "–û–∂–∏—Ä—ñ–Ω–Ω—è - –Ω–µ–æ–±—Ö—ñ–¥–Ω–µ —Å—Ö—É–¥–Ω–µ–Ω–Ω—è –¥–ª—è –∑–¥–æ—Ä–æ–≤'—è"
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É get_bmi_category: {e}")
       return "–ù–µ–º–æ–∂–ª–∏–≤–æ –≤–∏–∑–Ω–∞—á–∏—Ç–∏ –∫–∞—Ç–µ–≥–æ—Ä—ñ—é"

# –î–æ–¥–∞—î–º–æ –≤—ñ–¥—Å—É—Ç–Ω—ñ —Ñ—É–Ω–∫—Ü—ñ—ó
async def cancel(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   await update.message.reply_text(
       "–û–ø–µ—Ä–∞—Ü—ñ—é —Å–∫–∞—Å–æ–≤–∞–Ω–æ. –©–æ–± –ø–æ—á–∞—Ç–∏ –∑–Ω–æ–≤—É, –≤–∏–∫–æ—Ä–∏—Å—Ç–∞–π—Ç–µ –∫–æ–º–∞–Ω–¥—É /start"
   )
   return ConversationHandler.END

async def error_handler(update: object, context: ContextTypes.DEFAULT_TYPE) -> None:
   logger.error(f"–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞: {context.error}")
   try:
       if update and isinstance(update, Update) and update.effective_message:
           await update.effective_message.reply_text(
               "–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—ñ–¥ —á–∞—Å –≤–∏–∫–æ–Ω–∞–Ω–Ω—è –∫–æ–º–∞–Ω–¥–∏. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start"
           )
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –ø—Ä–∏ –æ–±—Ä–æ–±—Ü—ñ –ø–æ–º–∏–ª–∫–∏: {e}")

async def handle_hormone_menu(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   await update.message.reply_text(
       "–ú–µ–Ω—é –≥–æ—Ä–º–æ–Ω—ñ–≤ —â–µ –≤ —Ä–æ–∑—Ä–æ–±—Ü—ñ. –ü–æ–≤–µ—Ä–Ω—ñ—Ç—å—Å—è –¥–æ –ø–æ—á–∞—Ç–∫—É: /start"
   )
   return ConversationHandler.END

# –§—É–Ω–∫—Ü—ñ—è –∑–∞–ø—É—Å–∫—É –±–æ—Ç–∞ –∑ –æ–±—Ä–æ–±–∫–æ—é –ø–æ–º–∏–ª–æ–∫
async def start(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       user = update.effective_user
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {user.id} –∑–∞–ø—É—Å—Ç–∏–≤ –±–æ—Ç–∞")

       # –Ü–Ω—ñ—Ü—ñ–∞–ª—ñ–∑—É—î–º–æ –¥–∞–Ω—ñ –∫–æ—Ä–∏—Å—Ç—É–≤–∞—á–∞
       context.user_data.clear()  # –û—á–∏—â–∞—î–º–æ –ø–æ–ø–µ—Ä–µ–¥–Ω—ñ –¥–∞–Ω—ñ

       context.user_data['scores'] = {nt: 0 for nt in neurotransmitters}
       context.user_data['max_scores'] = {nt: len(questions[nt]) * 2 for nt in neurotransmitters}
       context.user_data['current_nt'] = None  # –ë—É–¥–µ –≤—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ –ø—ñ–∑–Ω—ñ—à–µ
       context.user_data['current_question_index'] = 0
       context.user_data['question_type'] = 'main'  # –ü–æ—á–∏–Ω–∞—î–º–æ –∑ –æ—Å–Ω–æ–≤–Ω–∏—Ö –ø–∏—Ç–∞–Ω—å
       context.user_data['extra_needed'] = {nt: False for nt in neurotransmitters}

       # –†–æ–∑—à–∏—Ä–µ–Ω–µ –ø—Ä–∏–≤—ñ—Ç–∞–ª—å–Ω–µ –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è
       await update.message.reply_text(
           f"–ü—Ä–∏–≤—ñ—Ç! –Ø *\"BroDiagnostic Brain Test Bot\"* üß¨\n\n"
           f"–¶–µ–π —Ç–µ—Å—Ç –±–∞–∑—É—î—Ç—å—Å—è –Ω–∞ –º–µ—Ç–æ–¥–∏—Ü—ñ –¥–æ–∫—Ç–æ—Ä–∞ –ë—Ä–∞–≤–µ—Ä–º–∞–Ω–∞ —Ç–∞ –≤–∫–ª—é—á–∞—î –∞–Ω–∞–ª—ñ–∑ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä—ñ–≤ —ñ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é.\n\n"
           f"–Ø –¥–æ–ø–æ–º–æ–∂—É —Ç–æ–±—ñ –æ—Ü—ñ–Ω–∏—Ç–∏ —Å—Ç–∞–Ω –∫–ª—é—á–æ–≤–∏—Ö –Ω–µ–π—Ä–æ—Ö—ñ–º—ñ—á–Ω–∏—Ö —Å–∏—Å—Ç–µ–º, —â–æ –≤–ø–ª–∏–≤–∞—é—Ç—å –Ω–∞ —Ç–≤–æ—î —Å–∞–º–æ–ø–æ—á—É—Ç—Ç—è, —Å–æ–Ω, –Ω–∞—Å—Ç—Ä—ñ–π, –µ–Ω–µ—Ä–≥—ñ—é —Ç–∞ –∫–æ–≥–Ω—ñ—Ç–∏–≤–Ω—ñ –∑–¥—ñ–±–Ω–æ—Å—Ç—ñ.\n\n"
           f"–ü—ñ—Å–ª—è –ø—Ä–æ—Ö–æ–¥–∂–µ–Ω–Ω—è —Ç–µ—Å—Ç—É —Ç–∏ –æ—Ç—Ä–∏–º–∞—î—à:\n"
           f"‚Ä¢ –ü–æ–≤–Ω–∏–π –Ω–µ–π—Ä–æ—Ö—ñ–º—ñ—á–Ω–∏–π –ø—Ä–æ—Ñ—ñ–ª—å üß†\n"
           f"‚Ä¢ –î–µ—Ç–∞–ª—å–Ω–∏–π –∞–Ω–∞–ª—ñ–∑ –∫–æ–∂–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞ —Ç–∞ –π–æ–≥–æ —Ä—ñ–≤–Ω—è\n"
           f"‚Ä¢ –ü–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –¥–ª—è –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—ó –±–∞–ª–∞–Ω—Å—É üí™\n"
           f"‚Ä¢ –†–æ–∑—É–º—ñ–Ω–Ω—è –≤–∑–∞—î–º–æ–∑–≤'—è–∑–∫—É –º—ñ–∂ —Å–ø–æ—Å–æ–±–æ–º –∂–∏—Ç—Ç—è —Ç–∞ –±—ñ–æ—Ö—ñ–º—ñ—î—é –º–æ–∑–∫—É\n\n"
           f"–©–æ–± –æ—Ç—Ä–∏–º–∞—Ç–∏ –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ —Ç–æ—á–Ω—ñ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏, –≤—ñ–¥–ø–æ–≤—ñ–¥–∞–π –Ω–∞ –ø–∏—Ç–∞–Ω–Ω—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ —á–µ—Å–Ω–æ.\n\n"
           f"–†–æ–∑–ø–æ—á–Ω—ñ–º–æ?",
           parse_mode='Markdown'
       )

       # –ü–∏—Ç–∞—î–º–æ —ñ–º'—è
       await update.message.reply_text(
           "–Ø–∫ —Ç–µ–±–µ –∑–≤–∞—Ç–∏?",
           reply_markup=ReplyKeyboardRemove()
       )

       return NAME
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É —Ñ—É–Ω–∫—Ü—ñ—ó start: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ —ñ–º–µ–Ω—ñ
async def handle_name(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       context.user_data['name'] = update.message.text
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤–∫–∞–∑–∞–≤ —ñ–º'—è: {context.user_data['name']}")

       # –ü–∏—Ç–∞—î–º–æ –ø—Ä–æ –≤—ñ–∫
       keyboard = [
           ["–î–æ 25 —Ä–æ–∫—ñ–≤", "25-35 —Ä–æ–∫—ñ–≤"],
           ["36-45 —Ä–æ–∫—ñ–≤", "46-60 —Ä–æ–∫—ñ–≤"],
           ["60+ —Ä–æ–∫—ñ–≤"]
       ]
       await update.message.reply_text(
           f"–ü—Ä–∏—î–º–Ω–æ –ø–æ–∑–Ω–∞–π–æ–º–∏—Ç–∏—Å—å, {context.user_data['name']}! –í–∫–∞–∂–∏, –±—É–¥—å –ª–∞—Å–∫–∞, —Å–≤—ñ–π –≤—ñ–∫:",
           reply_markup=ReplyKeyboardMarkup(keyboard, one_time_keyboard=True, resize_keyboard=True)
       )

       return AGE
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É handle_name: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ –≤—ñ–∫—É
async def handle_age(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       context.user_data['age'] = update.message.text
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤–∫–∞–∑–∞–≤ –≤—ñ–∫: {context.user_data['age']}")

       # –ü–∏—Ç–∞—î–º–æ –ø—Ä–æ —Å—Ç–∞—Ç—å
       keyboard = [["–ß–æ–ª–æ–≤—ñ–∫", "–ñ—ñ–Ω–∫–∞"]]
       await update.message.reply_text(
           "–í–∫–∞–∂–∏ —Å–≤–æ—é —Å—Ç–∞—Ç—å:",
           reply_markup=ReplyKeyboardMarkup(keyboard, one_time_keyboard=True, resize_keyboard=True)
       )

       return GENDER
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É handle_age: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ —Å—Ç–∞—Ç—ñ
async def handle_gender(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       context.user_data['gender'] = update.message.text
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤–∫–∞–∑–∞–≤ —Å—Ç–∞—Ç—å: {context.user_data['gender']}")

       # –ü–∏—Ç–∞—î–º–æ –ø—Ä–æ —Ç–∏–ø —Å—Ç–∞—Ç—É—Ä–∏
       keyboard = [
           ["–ü–æ–≤–Ω–∏–π", "–°–µ—Ä–µ–¥–Ω—ñ–π"],
           ["–°–ø–æ—Ä—Ç–∏–≤–Ω–∏–π", "–•—É–¥–∏–π"]
       ]
       await update.message.reply_text(
           "–Ø–∫–∏–π —É —Ç–µ–±–µ —Ç–∏–ø —Å—Ç–∞—Ç—É—Ä–∏?",
           reply_markup=ReplyKeyboardMarkup(keyboard, one_time_keyboard=True, resize_keyboard=True)
       )

       return BODY_TYPE
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É handle_gender: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ —Ç–∏–ø—É —Å—Ç–∞—Ç—É—Ä–∏
async def handle_body_type(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       context.user_data['body_type'] = update.message.text
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤–∫–∞–∑–∞–≤ —Ç–∏–ø —Å—Ç–∞—Ç—É—Ä–∏: {context.user_data['body_type']}")

       # –ü–∏—Ç–∞—î–º–æ –ø—Ä–æ –∑—Ä—ñ—Å—Ç
       await update.message.reply_text(
           "–í–∫–∞–∂–∏ —Å–≤—ñ–π –∑—Ä—ñ—Å—Ç —É —Å–∞–Ω—Ç–∏–º–µ—Ç—Ä–∞—Ö (–Ω–∞–ø—Ä–∏–∫–ª–∞–¥, 175):",
           reply_markup=ReplyKeyboardRemove()
       )

       return HEIGHT
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É handle_body_type: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ –∑—Ä–æ—Å—Ç—É
async def handle_height(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       height = int(update.message.text)
       if height < 50 or height > 250:
           await update.message.reply_text("–ë—É–¥—å –ª–∞—Å–∫–∞, –≤–∫–∞–∂–∏ –ø—Ä–∞–≤–∏–ª—å–Ω–∏–π –∑—Ä—ñ—Å—Ç —É —Å–∞–Ω—Ç–∏–º–µ—Ç—Ä–∞—Ö (–Ω–∞–ø—Ä–∏–∫–ª–∞–¥, 175):")
           return HEIGHT

       context.user_data['height'] = height
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤–∫–∞–∑–∞–≤ –∑—Ä—ñ—Å—Ç: {height} —Å–º")

       # –ü–∏—Ç–∞—î–º–æ –ø—Ä–æ –≤–∞–≥—É
       await update.message.reply_text(
           "–í–∫–∞–∂–∏ —Å–≤–æ—é –≤–∞–≥—É –≤ –∫—ñ–ª–æ–≥—Ä–∞–º–∞—Ö (–Ω–∞–ø—Ä–∏–∫–ª–∞–¥, 70):",
           reply_markup=ReplyKeyboardRemove()
       )

       return WEIGHT

   except ValueError:
       await update.message.reply_text("–í–≤–µ–¥–∏, –±—É–¥—å –ª–∞—Å–∫–∞, —á–∏—Å–ª–æ. –Ø–∫–∏–π —Ç–≤—ñ–π –∑—Ä—ñ—Å—Ç —É —Å–∞–Ω—Ç–∏–º–µ—Ç—Ä–∞—Ö?")
       return HEIGHT
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É handle_height: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ –≤–∞–≥–∏
async def handle_weight(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       weight = float(update.message.text)
       if weight < 20 or weight > 300:
           await update.message.reply_text("–ë—É–¥—å –ª–∞—Å–∫–∞, –≤–∫–∞–∂–∏ –ø—Ä–∞–≤–∏–ª—å–Ω—É –≤–∞–≥—É –≤ –∫—ñ–ª–æ–≥—Ä–∞–º–∞—Ö (–Ω–∞–ø—Ä–∏–∫–ª–∞–¥, 70):")
           return WEIGHT

       context.user_data['weight'] = weight
       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤–∫–∞–∑–∞–≤ –≤–∞–≥—É: {weight} –∫–≥")

       # –†–æ–∑—Ä–∞—Ö—É–≤–∞—Ç–∏ –Ü–ú–¢
       height = context.user_data['height']
       body_type = context.user_data['body_type']
       bmi = calculate_bmi(weight, height)
       bmi_category = get_bmi_category(bmi, body_type, weight)

       context.user_data['bmi'] = bmi
       context.user_data['bmi_category'] = bmi_category

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –ø–µ—Ä–µ–¥ –ø–æ—á–∞—Ç–∫–æ–º —Ç–µ—Å—Ç—É –∑ —ñ–Ω—Ñ–æ—Ä–º–∞—Ü—ñ—î—é –ø—Ä–æ –Ü–ú–¢
       await update.message.reply_text(
           f"–î—è–∫—É—é! –¢–≤—ñ–π —ñ–Ω–¥–µ–∫—Å –º–∞—Å–∏ —Ç—ñ–ª–∞ (–Ü–ú–¢): *{bmi}* - *{bmi_category}*.\n\n"
           f"–¢–µ–ø–µ—Ä –ø–µ—Ä–µ–π–¥–µ–º–æ –¥–æ –æ—Ü—ñ–Ω–∫–∏ —Ç–≤–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–Ω–æ–≥–æ —Ç–∞ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é.\n\n"
           f"–Ø –∑–∞–¥–∞–º —Å–µ—Ä—ñ—é –ø–∏—Ç–∞–Ω—å –ø—Ä–æ —Ç–≤–æ—î —Å–∞–º–æ–ø–æ—á—É—Ç—Ç—è, –Ω–∞—Å—Ç—Ä—ñ–π, –µ–Ω–µ—Ä–≥—ñ—é —Ç–∞ –∫–æ–≥–Ω—ñ—Ç–∏–≤–Ω—ñ —Ñ—É–Ω–∫—Ü—ñ—ó. "
           f"–í—ñ–¥–ø–æ–≤—ñ–¥–∞–π –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ —á–µ—Å–Ω–æ, —â–æ–± –æ—Ç—Ä–∏–º–∞—Ç–∏ —Ç–æ—á–Ω—ñ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏.",
           parse_mode='Markdown'
       )

       # –ó–∞—Ç—Ä–∏–º–∫–∞ –ø–µ—Ä–µ–¥ –ø–æ—á–∞—Ç–∫–æ–º —Ç–µ—Å—Ç—É –¥–ª—è –∫—Ä–∞—â–æ–≥–æ —Å–ø—Ä–∏–π–Ω—è—Ç—Ç—è
       time.sleep(1)

       # –í–∏–∑–Ω–∞—á–∞—î–º–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∏ —Ç–∞ –≥–æ—Ä–º–æ–Ω–∏, —è–∫—ñ –ø–æ–∫–∞–∑—É–≤–∞—Ç–∏–º–µ–º–æ –∫–æ—Ä–∏—Å—Ç—É–≤–∞—á—É
       available_items = []

       # –°–ø–æ—á–∞—Ç–∫—É –¥–æ–¥–∞—î–º–æ –≤—Å—ñ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∏
       for nt, data in neurotransmitters.items():
           if data['type'] == 'neurotransmitter':
               available_items.append(nt)

       # –ü–æ—Ç—ñ–º –¥–æ–¥–∞—î–º–æ —Å–∏—Å—Ç–µ–º–Ω—ñ –±–ª–æ–∫–∏ (–ø–µ—á—ñ–Ω–∫–∞, –∫–æ—Ä—Ç–∏–∑–æ–ª, —Å–æ–Ω)
       for nt, data in neurotransmitters.items():
           if data['type'] == 'system':
               available_items.append(nt)

       # –ù–∞—Ä–µ—à—Ç—ñ –¥–æ–¥–∞—î–º–æ –≥–æ—Ä–º–æ–Ω–∏ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω–æ –¥–æ —Å—Ç–∞—Ç—ñ
       gender = context.user_data['gender']

       if gender == "–ß–æ–ª–æ–≤—ñ–∫":
           # –î–ª—è —á–æ–ª–æ–≤—ñ–∫—ñ–≤ –¥–æ–¥–∞—î–º–æ —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω, –µ—Å—Ç—Ä–∞–¥—ñ–æ–ª —á–æ–ª–æ–≤—ñ—á–∏–π —ñ –ø—Ä–æ–ª–∞–∫—Ç–∏–Ω
           for nt, data in neurotransmitters.items():
               if data['type'] == 'hormone':
                   if 'gender_specific' not in data or data['gender_specific'] == 'male':
                       # –î–æ–¥–∞–Ω–æ –ø–µ—Ä–µ–≤—ñ—Ä–∫—É, —â–æ —Ü–µ –Ω–µ –∂—ñ–Ω–æ—á–∏–π –≥–æ—Ä–º–æ–Ω
                       if nt != 'female_hormones' and nt != 'estradiol_female':
                           available_items.append(nt)
       else:
           # –î–ª—è –∂—ñ–Ω–æ–∫ –¥–æ–¥–∞—î–º–æ –∂—ñ–Ω–æ—á—ñ –≥–æ—Ä–º–æ–Ω–∏, –µ—Å—Ç—Ä–∞–¥—ñ–æ–ª –∂—ñ–Ω–æ—á–∏–π —ñ –ø—Ä–æ–ª–∞–∫—Ç–∏–Ω
           for nt, data in neurotransmitters.items():
               if data['type'] == 'hormone':
                   if 'gender_specific' not in data or data['gender_specific'] == 'female':
                       # –î–æ–¥–∞–Ω–æ –ø–µ—Ä–µ–≤—ñ—Ä–∫—É, —â–æ —Ü–µ –Ω–µ —á–æ–ª–æ–≤—ñ—á–∏–π –≥–æ—Ä–º–æ–Ω
                       if nt != 'testosterone' and nt != 'estradiol_male':
                           available_items.append(nt)

       context.user_data['available_items'] = available_items
       context.user_data['current_nt'] = available_items[0]

       # –û—á—ñ–∫—É–≤–∞–Ω–Ω—è –ø–µ—Ä–µ–¥ –ø–µ—Ä—à–∏–º –∑–∞–ø–∏—Ç–∞–Ω–Ω—è–º
       await update.message.reply_text(
           "üìä –ü–æ—á–∏–Ω–∞—é –Ω–µ–π—Ä–æ—Ö—ñ–º—ñ—á–Ω–∏–π —Ç–µ—Å—Ç...",
       )

       time.sleep(0.5)

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –ø—Ä–æ –ø–æ—á–∞—Ç–æ–∫ –∑ –ø–µ—Ä—à–∏–º –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–æ–º
       current_nt = context.user_data['current_nt']
       await update.message.reply_text(
           f"{neurotransmitters[current_nt]['title']}\n\n"
           f"–ó–∞—Ä–∞–∑ —è –ø–æ—Å—Ç–∞–≤–ª—é —Ç–æ–±—ñ 5 –æ—Å–Ω–æ–≤–Ω–∏—Ö –ø–∏—Ç–∞–Ω—å –ø—Ä–æ {neurotransmitters[current_nt]['name'].lower()}.\n\n"
           f"–ü–∏—Ç–∞–Ω–Ω—è 1/{len(questions[current_nt])}:"
       )

       # –ó–∞–¥–∞—î–º–æ –ø–µ—Ä—à–µ –ø–∏—Ç–∞–Ω–Ω—è
       await ask_question(update, context)

       return QUESTIONS

   except ValueError:
       await update.message.reply_text("–í–≤–µ–¥–∏, –±—É–¥—å –ª–∞—Å–∫–∞, —á–∏—Å–ª–æ. –Ø–∫–∞ —Ç–≤–æ—è –≤–∞–≥–∞ –≤ –∫—ñ–ª–æ–≥—Ä–∞–º–∞—Ö?")
       return WEIGHT
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ —É handle_weight: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –§—É–Ω–∫—Ü—ñ—è –¥–ª—è –∑–∞–¥–∞–≤–∞–Ω–Ω—è –ø–∏—Ç–∞–Ω—å
async def ask_question(update: Update, context: ContextTypes.DEFAULT_TYPE):
   try:
       current_nt = context.user_data.get('current_nt')
       if not current_nt:
           logger.error("current_nt —î None –≤ ask_question")
           await update.message.reply_text("–°—Ç–∞–ª–∞—Å—è –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –ø–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç–∏ –±–æ—Ç–∞ /start")
           return ConversationHandler.END

       question_type = context.user_data.get('question_type', 'main')
       question_index = context.user_data.get('current_question_index', 0)

       logger.info(f"ask_question: {current_nt}, —Ç–∏–ø: {question_type}, —ñ–Ω–¥–µ–∫—Å: {question_index}")

       # –ü–µ—Ä–µ–≤—ñ—Ä–∫–∞ –Ω–∞ –Ω–∞—è–≤–Ω—ñ—Å—Ç—å –ø–∏—Ç–∞–Ω—å –¥–ª—è –ø–æ—Ç–æ—á–Ω–æ–≥–æ nt
       if current_nt not in questions:
           logger.error(f"–ù–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä {current_nt} –≤—ñ–¥—Å—É—Ç–Ω—ñ–π —É —Å–ª–æ–≤–Ω–∏–∫—É –ø–∏—Ç–∞–Ω—å")
           await update.message.reply_text("–°—Ç–∞–ª–∞—Å—è –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –ø–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç–∏ –±–æ—Ç–∞ /start")
           return ConversationHandler.END

       question_list = questions[current_nt] if question_type == 'main' else extra_questions.get(current_nt, [])

       # –ü–µ—Ä–µ–≤—ñ—Ä–∫–∞ –Ω–∞ –Ω–∞—è–≤–Ω—ñ—Å—Ç—å –¥–æ–¥–∞—Ç–∫–æ–≤–∏—Ö –ø–∏—Ç–∞–Ω—å
       if question_type == 'extra' and (current_nt not in extra_questions or not extra_questions[current_nt]):
           logger.warning(f"–ù–µ–º–∞—î –¥–æ–¥–∞—Ç–∫–æ–≤–∏—Ö –ø–∏—Ç–∞–Ω—å –¥–ª—è {current_nt}, –ø–µ—Ä–µ—Ö–æ–¥–∏–º–æ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞")
           return await move_to_next_neurotransmitter(update, context)

       # –ü–µ—Ä–µ–≤—ñ—Ä—è—î–º–æ, —á–∏ –Ω–µ –∑–∞–∫—ñ–Ω—á–∏–ª–∏—Å—å –ø–∏—Ç–∞–Ω–Ω—è
       if question_index >= len(question_list):
           logger.info(f"–ü–∏—Ç–∞–Ω–Ω—è –∑–∞–∫—ñ–Ω—á–∏–ª–∏—Å—å –¥–ª—è {current_nt}, —Ç–∏–ø: {question_type}")
           # –Ø–∫—â–æ —Ü–µ –±—É–ª–∏ –æ—Å–Ω–æ–≤–Ω—ñ –ø–∏—Ç–∞–Ω–Ω—è, –ø–µ—Ä–µ–≤—ñ—Ä—è—î–º–æ, —á–∏ –ø–æ—Ç—Ä—ñ–±–Ω—ñ –¥–æ–¥–∞—Ç–∫–æ–≤—ñ
           if question_type == 'main':
               score = context.user_data['scores'].get(current_nt, 0)
               threshold = neurotransmitters[current_nt].get('threshold', 5)

               logger.info(f"–û—Å–Ω–æ–≤–Ω—ñ –ø–∏—Ç–∞–Ω–Ω—è –∑–∞–∫—ñ–Ω—á–∏–ª–∏—Å—å –¥–ª—è {current_nt}, –±–∞–ª: {score}, –ø–æ—Ä—ñ–≥: {threshold}")

               if score <= threshold:  # –ë–∞–ª–∏ –º–µ–Ω—à–µ –∞–±–æ –¥–æ—Ä—ñ–≤–Ω—é—é—Ç—å –ø–æ—Ä–æ–≥—É –≤–∫–∞–∑—É—é—Ç—å –Ω–∞ –ø—Ä–æ–±–ª–µ–º—É
                   # –î–æ–¥–∞—Ç–∫–æ–≤—ñ –ø–∏—Ç–∞–Ω–Ω—è –ø–æ—Ç—Ä—ñ–±–Ω—ñ
                   context.user_data['extra_needed'][current_nt] = True
                   context.user_data['question_type'] = 'extra'
                   context.user_data['current_question_index'] = 0

                   # –ü–µ—Ä–µ–≤—ñ—Ä–∫–∞ –Ω–∞ –Ω–∞—è–≤–Ω—ñ—Å—Ç—å –¥–æ–¥–∞—Ç–∫–æ–≤–∏—Ö –ø–∏—Ç–∞–Ω—å
                   if current_nt not in extra_questions or not extra_questions[current_nt]:
                       logger.warning(f"–ù–µ–º–∞—î –¥–æ–¥–∞—Ç–∫–æ–≤–∏—Ö –ø–∏—Ç–∞–Ω—å –¥–ª—è {current_nt}, —Ö–æ—á–∞ –ø–æ—Ç—Ä—ñ–±–Ω—ñ –¥–æ–¥–∞—Ç–∫–æ–≤—ñ –ø–∏—Ç–∞–Ω–Ω—è")
                       return await move_to_next_neurotransmitter(update, context)

                   # –û–Ω–æ–≤–ª—é—î–º–æ –º–∞–∫—Å–∏–º–∞–ª—å–Ω–∏–π –º–æ–∂–ª–∏–≤–∏–π –±–∞–ª –¥–ª—è —Ü—å–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
                   context.user_data['max_scores'][current_nt] += len(extra_questions[current_nt]) * 2

                   # –í–∏–±–∏—Ä–∞—î–º–æ –≤–∏–ø–∞–¥–∫–æ–≤—É —Ñ—Ä–∞–∑—É –∑ processing_phrases_middle
                   thinking_phrase = random.choice(processing_phrases_middle)

                   await update.message.reply_text(
                       f"üìä {thinking_phrase}"
                   )

                   # –Ü–º—ñ—Ç—É—î–º–æ –æ–±—Ä–æ–±–∫—É
                   time.sleep(1.5)

                   await update.message.reply_text(
                       f"–í–∏—è–≤–ª–µ–Ω–æ –º–æ–∂–ª–∏–≤–∏–π –¥–∏—Å–±–∞–ª–∞–Ω—Å —Ä—ñ–≤–Ω—è {neurotransmitters[current_nt]['name']}. "
                       f"–ü–æ—Ç—Ä—ñ–±–Ω–æ –¥–µ—Ç–∞–ª—å–Ω—ñ—à–µ –¥–æ—Å–ª—ñ–¥–∏—Ç–∏ —Ü–µ–π –ø–æ–∫–∞–∑–Ω–∏–∫."
                   )

                   await update.message.reply_text(
                       f"–î–æ–¥–∞—Ç–∫–æ–≤—ñ –ø–∏—Ç–∞–Ω–Ω—è –¥–ª—è –≥–ª–∏–±—à–æ–≥–æ –∞–Ω–∞–ª—ñ–∑—É (1/{len(extra_questions[current_nt])}):"
                   )

                   # –ó–∞–¥–∞—î–º–æ –ø–µ—Ä—à–µ –¥–æ–¥–∞—Ç–∫–æ–≤–µ –ø–∏—Ç–∞–Ω–Ω—è
                   return await ask_question(update, context)
               else:
                   # –î–æ–¥–∞—Ç–∫–æ–≤—ñ –ø–∏—Ç–∞–Ω–Ω—è –Ω–µ –ø–æ—Ç—Ä—ñ–±–Ω—ñ, –ø–µ—Ä–µ—Ö–æ–¥–∏–º–æ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
                   return await move_to_next_neurotransmitter(update, context)
           else:
               # –¶–µ –±—É–ª–∏ –¥–æ–¥–∞—Ç–∫–æ–≤—ñ –ø–∏—Ç–∞–Ω–Ω—è, –ø–µ—Ä–µ—Ö–æ–¥–∏–º–æ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
               return await move_to_next_neurotransmitter(update, context)

       # –û—Ç—Ä–∏–º—É—î–º–æ –ø–æ—Ç–æ—á–Ω–µ –ø–∏—Ç–∞–Ω–Ω—è
       question = question_list[question_index]

       # –í—ñ–¥–ø—Ä–∞–≤–ª—è—î–º–æ –ø–∏—Ç–∞–Ω–Ω—è
       keyboard = [["–¢–∞–∫", "–Ü–Ω–∫–æ–ª–∏", "–ù—ñ"]]
       await update.message.reply_text(
           question,
           reply_markup=ReplyKeyboardMarkup(keyboard, one_time_keyboard=True, resize_keyboard=True)
       )

       return QUESTIONS  # –ü–æ–≤–µ—Ä—Ç–∞—î–º–æ —Ç–æ–π —Å–∞–º–∏–π —Å—Ç–∞–Ω –¥–ª—è –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –ø–∏—Ç–∞–Ω–Ω—è
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ ask_question: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—ñ–¥ —á–∞—Å –æ–±—Ä–æ–±–∫–∏ –ø–∏—Ç–∞–Ω–Ω—è. –°–ø—Ä–æ–±—É–π—Ç–µ —â–µ —Ä–∞–∑ /start")
       return ConversationHandler.END

# –§—É–Ω–∫—Ü—ñ—è –¥–ª—è –ø–µ—Ä–µ—Ö–æ–¥—É –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
async def move_to_next_neurotransmitter(update: Update, context: ContextTypes.DEFAULT_TYPE):
   try:
       current_nt = context.user_data.get('current_nt')
       available_items = context.user_data.get('available_items', [])

       if not current_nt or not available_items:
           logger.error(f"current_nt –∞–±–æ available_items —î None –≤ move_to_next_neurotransmitter")
           await update.message.reply_text("–°—Ç–∞–ª–∞—Å—è –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –ø–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç–∏ –±–æ—Ç–∞ /start")
           return ConversationHandler.END

       current_index = available_items.index(current_nt)

       # –î–æ–¥–∞–º–æ –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –∑ –ø—Ä–æ–º—ñ–∂–Ω–∏–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–º –ø–æ—Ç–æ—á–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
       score = context.user_data['scores'].get(current_nt, 0)
       max_score = context.user_data['max_scores'].get(current_nt, 10)  # –ó–∞ –∑–∞–º–æ–≤—á—É–≤–∞–Ω–Ω—è–º 10, —â–æ–± —É–Ω–∏–∫–Ω—É—Ç–∏ –¥—ñ–ª–µ–Ω–Ω—è –Ω–∞ 0
       percentage = (score / max_score) * 100 if max_score > 0 else 0

       # –í–∏–∑–Ω–∞—á–∞—î–º–æ —Ä—ñ–≤–µ–Ω—å
       if percentage >= 75:
           level = "üü¢ –í—ñ–¥–º—ñ–Ω–Ω–æ"
       elif percentage >= 50:
           level = "üü° –ù–æ—Ä–º–∞–ª—å–Ω–æ"
       elif percentage >= 25:
           level = "üü† –ü–æ—Ç—Ä–µ–±—É—î —É–≤–∞–≥–∏"
       else:
           level = "üî¥ –ö—Ä–∏—Ç–∏—á–Ω–æ"

       # –í—ñ–¥–ø—Ä–∞–≤–ª—è—î–º–æ –ø—Ä–æ–º—ñ–∂–Ω–∏–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç
       await update.message.reply_text(
           f"üìä *–ü—Ä–æ–º—ñ–∂–Ω–∏–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç –¥–ª—è {neurotransmitters[current_nt]['name']}*\n\n"
           f"–†—ñ–≤–µ–Ω—å: {level} ({int(percentage)}%)\n\n"
           f"_{neurotransmitters[current_nt]['description']}_",
           parse_mode='Markdown'
       )

       # –ü–µ—Ä–µ–≤—ñ—Ä—è—î–º–æ, —á–∏ —î —â–µ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∏/–≥–æ—Ä–º–æ–Ω–∏ –¥–ª—è —Ç–µ—Å—Ç—É–≤–∞–Ω–Ω—è
       if current_index < len(available_items) - 1:
           # –í–∏–±–∏—Ä–∞—î–º–æ –≤–∏–ø–∞–¥–∫–æ–≤—É —Ñ—Ä–∞–∑—É –∑ processing_phrases_middle –∑ –π–º–æ–≤—ñ—Ä–Ω—ñ—Å—Ç—é 30%
           if random.random() < 0.3:
               thinking_phrase = random.choice(processing_phrases_middle)
               await update.message.reply_text(f"{thinking_phrase}")
               time.sleep(1.0)

           # –ü–µ—Ä–µ—Ö–æ–¥–∏–º–æ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞/–≥–æ—Ä–º–æ–Ω—É
           next_nt = available_items[current_index + 1]
           context.user_data['current_nt'] = next_nt
           context.user_data['question_type'] = 'main'
           context.user_data['current_question_index'] = 0

           # –ü–æ–∫–∞–∑—É—î–º–æ –ø—Ä–æ–≥—Ä–µ—Å
           total_items = len(available_items)
           progress = current_index + 2  # +1 –±–æ —ñ–Ω–¥–µ–∫—Å –∑ 0, +1 –±–æ –ø–µ—Ä–µ–π—à–ª–∏ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ

           await update.message.reply_text(
               f"üìà *–ü—Ä–æ–≥—Ä–µ—Å: {progress}/{total_items}*",
               parse_mode='Markdown'
           )

           # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –ø—Ä–æ –∑–º—ñ–Ω—É –∫–∞—Ç–µ–≥–æ—Ä—ñ—ó
           item_type = "–Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä" if neurotransmitters[next_nt]['type'] == 'neurotransmitter' else (
               "–≥–æ—Ä–º–æ–Ω" if neurotransmitters[next_nt]['type'] == 'hormone' else "–ø–æ–∫–∞–∑–Ω–∏–∫"
           )

           await update.message.reply_text(
               f"{neurotransmitters[next_nt]['title']}\n\n"
               f"–¢–µ–ø–µ—Ä —è –ø–æ—Å—Ç–∞–≤–ª—é —Ç–æ–±—ñ 5 –æ—Å–Ω–æ–≤–Ω–∏—Ö –ø–∏—Ç–∞–Ω—å –ø—Ä–æ —Ü–µ–π {item_type}.\n\n"
               f"–ü–∏—Ç–∞–Ω–Ω—è 1/{len(questions[next_nt])}:"
           )

           # –ó–∞–¥–∞—î–º–æ –ø–µ—Ä—à–µ –ø–∏—Ç–∞–Ω–Ω—è –Ω–æ–≤–æ—ó –∫–∞—Ç–µ–≥–æ—Ä—ñ—ó
           return await ask_question(update, context)
       else:
           # –¢–µ—Å—Ç –∑–∞–≤–µ—Ä—à–µ–Ω–æ, –ø–æ–∫–∞–∑—É—î–º–æ "–±–æ—Ç –¥—É–º–∞—î" —Ç–∞ –ø–æ—Ç—ñ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏
           return await process_results(update, context)
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ move_to_next_neurotransmitter: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—Ä–∏ –ø–µ—Ä–µ—Ö–æ–¥—ñ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ—ó —á–∞—Å—Ç–∏–Ω–∏ —Ç–µ—Å—Ç—É. –°–ø—Ä–æ–±—É–π—Ç–µ —â–µ —Ä–∞–∑ /start")
       return ConversationHandler.END

# –û–±—Ä–æ–±–∫–∞ –≤—ñ–¥–ø–æ–≤—ñ–¥–µ–π
async def handle_answer(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       answer = update.message.text
       current_nt = context.user_data.get('current_nt')
       question_index = context.user_data.get('current_question_index', 0)

       if not current_nt:
           logger.error("current_nt —î None –≤ handle_answer")
           await update.message.reply_text("–°—Ç–∞–ª–∞—Å—è –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –ø–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç–∏ –±–æ—Ç–∞ /start")
           return ConversationHandler.END

       # –ù–∞—Ä–∞—Ö–æ–≤—É—î–º–æ –±–∞–ª–∏ –∑–∞–ª–µ–∂–Ω–æ –≤—ñ–¥ –≤—ñ–¥–ø–æ–≤—ñ–¥—ñ
       if answer == "–¢–∞–∫":
           score = 0
       elif answer == "–Ü–Ω–∫–æ–ª–∏":
           score = 1
       elif answer == "–ù—ñ":
           score = 2
       else:
           score = 0  # –î–ª—è –±–µ–∑–ø–µ–∫–∏

       # –î–æ–¥–∞—î–º–æ –±–∞–ª–∏ –¥–æ –∑–∞–≥–∞–ª—å–Ω–æ–≥–æ —Ä–∞—Ö—É–Ω–∫—É
       if 'scores' not in context.user_data or current_nt not in context.user_data['scores']:
           context.user_data.setdefault('scores', {})
           context.user_data['scores'][current_nt] = 0

       context.user_data['scores'][current_nt] += score

       logger.info(f"–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á {update.effective_user.id} –≤—ñ–¥–ø–æ–≤—ñ–≤ –Ω–∞ –ø–∏—Ç–∞–Ω–Ω—è {current_nt}: {answer}, –±–∞–ª: {score}")

       # –ü–µ—Ä–µ—Ö–æ–¥–∏–º–æ –¥–æ –Ω–∞—Å—Ç—É–ø–Ω–æ–≥–æ –ø–∏—Ç–∞–Ω–Ω—è
       context.user_data['current_question_index'] = question_index + 1

       # –ü–æ–∫–∞–∑—É—î–º–æ –ª—ñ—á–∏–ª—å–Ω–∏–∫ –ø–∏—Ç–∞–Ω—å, —è–∫—â–æ —î –Ω–∞—Å—Ç—É–ø–Ω–µ –ø–∏—Ç–∞–Ω–Ω—è
       question_type = context.user_data.get('question_type', 'main')
       question_list = questions[current_nt] if question_type == 'main' else extra_questions.get(current_nt, [])
       next_index = context.user_data['current_question_index'] + 1

       if next_index <= len(question_list):
           await update.message.reply_text(
               f"–ü–∏—Ç–∞–Ω–Ω—è {next_index}/{len(question_list)}:"
           )

       # –ó–∞–¥–∞—î–º–æ –Ω–∞—Å—Ç—É–ø–Ω–µ –ø–∏—Ç–∞–Ω–Ω—è
       await ask_question(update, context)

       return QUESTIONS
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ handle_answer: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—ñ–¥ —á–∞—Å –æ–±—Ä–æ–±–∫–∏ –≤—ñ–¥–ø–æ–≤—ñ–¥—ñ. –°–ø—Ä–æ–±—É–π—Ç–µ —â–µ —Ä–∞–∑ /start")
       return ConversationHandler.END

# –Ü–º—ñ—Ç–∞—Ü—ñ—è –æ–±—Ä–æ–±–∫–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤
async def process_results(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       await update.message.reply_text(
           "üß† *–¢–µ—Å—Ç –∑–∞–≤–µ—Ä—à–µ–Ω–æ!* –ü–æ—á–∏–Ω–∞—é –∞–Ω–∞–ª—ñ–∑ —Ç–≤–æ–≥–æ –Ω–µ–π—Ä–æ—Ö—ñ–º—ñ—á–Ω–æ–≥–æ —Ç–∞ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é...",
           parse_mode='Markdown'
       )

       # –Ü–º—ñ—Ç—É—î–º–æ –ø—Ä–æ—Ü–µ—Å –æ–±—Ä–æ–±–∫–∏ –∑ –∫—ñ–ª—å–∫–æ–º–∞ –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è–º–∏, –∞–ª–µ –º–µ–Ω—à–æ—é –∫—ñ–ª—å–∫—ñ—Å—Ç—é
       # –°–ø–æ—á–∞—Ç–∫—É 1-2 –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –∑ processing_phrases_beginning
       phrases_to_show = random.sample(processing_phrases_beginning, k=min(2, len(processing_phrases_beginning)))
       for phrase in phrases_to_show:
           time.sleep(random.uniform(1.0, 1.5))
           await update.message.reply_text(f"{phrase}")

       # –ü–æ—Ç—ñ–º 1-2 –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –∑ processing_phrases_middle
       phrases_to_show = random.sample(processing_phrases_middle, k=min(2, len(processing_phrases_middle)))
       for phrase in phrases_to_show:
           time.sleep(random.uniform(1.0, 1.5))
           await update.message.reply_text(f"{phrase}")

       # –ù–∞—Ä–µ—à—Ç—ñ 1-2 –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è –∑ processing_phrases_end
       phrases_to_show = random.sample(processing_phrases_end, k=min(2, len(processing_phrases_end)))
       for phrase in phrases_to_show:
           time.sleep(random.uniform(1.0, 1.5))
           await update.message.reply_text(f"{phrase}")

       # –ù–µ–≤–µ–ª–∏–∫–∞ –∑–∞—Ç—Ä–∏–º–∫–∞ –ø–µ—Ä–µ–¥ –ø–æ–∫–∞–∑–æ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤
       time.sleep(1.0)

       # –ü–æ–∫–∞–∑—É—î–º–æ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏
       return await show_results(update, context)
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ process_results: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—ñ–¥ —á–∞—Å –æ–±—Ä–æ–±–∫–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤. –°–ø—Ä–æ–±—É–π—Ç–µ —â–µ —Ä–∞–∑ /start")
       return ConversationHandler.END

# –ü–æ–∫–∞–∑ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ - —Ä–æ–∑–±–∏—Ç–∏–π –Ω–∞ –∫—ñ–ª—å–∫–∞ –ø–æ–≤—ñ–¥–æ–º–ª–µ–Ω—å
async def show_results(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       scores = context.user_data.get('scores', {})
       max_scores = context.user_data.get('max_scores', {})
       name = context.user_data.get('name', '–ö–æ—Ä–∏—Å—Ç—É–≤–∞—á')
       bmi = context.user_data.get('bmi', 0)
       bmi_category = context.user_data.get('bmi_category', '–ù–µ –≤–∏–∑–Ω–∞—á–µ–Ω–æ')
       body_type = context.user_data.get('body_type', '–ù–µ –≤–∏–∑–Ω–∞—á–µ–Ω–æ')

       # –í–∏–∫–æ—Ä–∏—Å—Ç–æ–≤—É—î–º–æ —Ç—ñ–ª—å–∫–∏ –¥–æ—Å—Ç—É–ø–Ω—ñ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∏/–≥–æ—Ä–º–æ–Ω–∏ –¥–ª—è –∞–Ω–∞–ª—ñ–∑—É
       available_items = context.user_data.get('available_items', [])

       # –†–æ–∑–¥—ñ–ª—è—î–º–æ –¥–æ—Å—Ç—É–ø–Ω—ñ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∏ —Ç–∞ –≥–æ—Ä–º–æ–Ω–∏
       available_neurotransmitters = [nt for nt in available_items if neurotransmitters[nt]['type'] == 'neurotransmitter']
       available_hormones = [nt for nt in available_items if neurotransmitters[nt]['type'] == 'hormone']
       available_systems = [nt for nt in available_items if neurotransmitters[nt]['type'] == 'system']

       # –†–æ–∑—Ä–∞—Ö–æ–≤—É—î–º–æ –≤—ñ–¥—Å–æ—Ç–∫–∏ –¥–ª—è –≤—Å—ñ—Ö –ø–æ–∫–∞–∑–Ω–∏–∫—ñ–≤
       percentage_scores = {}
       for nt in available_items:
           if nt in max_scores and max_scores[nt] > 0:
               percentage_scores[nt] = (scores.get(nt, 0) / max_scores[nt]) * 100
           else:
               percentage_scores[nt] = 0

       # –ó–Ω–∞—Ö–æ–¥–∏–º–æ –¥–æ–º—ñ–Ω—É—é—á–∏–π –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä —Ç–∞ –≥–æ—Ä–º–æ–Ω –æ–∫—Ä–µ–º–æ
       dominant_neurotransmitter = max([nt for nt in available_neurotransmitters],
                                      key=lambda x: percentage_scores.get(x, 0)) if available_neurotransmitters else None

       dominant_hormone = max([nt for nt in available_hormones],
                             key=lambda x: percentage_scores.get(x, 0)) if available_hormones else None

       # –ó–Ω–∞—Ö–æ–¥–∏–º–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä —Ç–∞ –≥–æ—Ä–º–æ–Ω –∑ –Ω–∞–π–Ω–∏–∂—á–∏–º –≤—ñ–¥—Å–æ—Ç–∫–æ–º –æ–∫—Ä–µ–º–æ
       deficient_neurotransmitter = min([nt for nt in available_neurotransmitters],
                                       key=lambda x: percentage_scores.get(x, 0)) if available_neurotransmitters else None

       deficient_hormone = min([nt for nt in available_hormones],
                              key=lambda x: percentage_scores.get(x, 0)) if available_hormones else None

       # –í–∏–∑–Ω–∞—á–∞—î–º–æ –¥–µ—Ñ—ñ—Ü–∏—Ç–Ω–∏–π —Å–∏—Å—Ç–µ–º–Ω–∏–π –±–ª–æ–∫
       deficient_system = min([nt for nt in available_systems],
                             key=lambda x: percentage_scores.get(x, 0)) if available_systems else None

       # –í–∏–∑–Ω–∞—á–∞—î–º–æ —Ä—ñ–≤–Ω—ñ –¥–ª—è –≤—ñ–∑—É–∞–ª—ñ–∑–∞—Ü—ñ—ó
       def get_level_emoji(percentage):
           if percentage >= 75:
               return "üü¢ –í—ñ–¥–º—ñ–Ω–Ω–æ"
           elif percentage >= 50:
               return "üü° –ù–æ—Ä–º–∞–ª—å–Ω–æ"
           elif percentage >= 25:
               return "üü† –ü–æ—Ç—Ä–µ–±—É—î —É–≤–∞–≥–∏"
           else:
               return "üî¥ –ö—Ä–∏—Ç–∏—á–Ω–æ"

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 1: –ó–∞–≥–æ–ª–æ–≤–æ–∫ —Ç–∞ –∞–Ω—Ç—Ä–æ–ø–æ–º–µ—Ç—Ä–∏—á–Ω—ñ –¥–∞–Ω—ñ
       result_text = f"üî¨ *–†–ï–ó–£–õ–¨–¢–ê–¢–ò –ù–ï–ô–†–û–•–Ü–ú–Ü–ß–ù–û–ì–û –ü–†–û–§–Ü–õ–Æ* üî¨\n\n"
       result_text += f"*{name}, –æ—Å—å —Ç–≤—ñ–π –ø–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω–∏–π –∞–Ω–∞–ª—ñ–∑:*\n\n"
       result_text += f"*–ê–ù–¢–†–û–ü–û–ú–ï–¢–†–ò–ß–ù–Ü –î–ê–ù–Ü:*\n"
       result_text += f"‚Ä¢ –¢–∏–ø —Å—Ç–∞—Ç—É—Ä–∏: {body_type}\n"
       result_text += f"‚Ä¢ –Ü–Ω–¥–µ–∫—Å –º–∞—Å–∏ —Ç—ñ–ª–∞ (–Ü–ú–¢): {bmi} - {bmi_category}\n\n"

       await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 2: –ù–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–Ω–∏–π –ø—Ä–æ—Ñ—ñ–ª—å
       if available_neurotransmitters:
           result_text = "*–ù–ï–ô–†–û–ú–ï–î–Ü–ê–¢–û–†–ù–ò–ô –ü–†–û–§–Ü–õ–¨:*\n\n"

           # –ü—Ä–æ—Ö–æ–¥–∏–º–æ –ø–æ –≤—Å—ñ—Ö –¥–æ—Å—Ç—É–ø–Ω–∏—Ö –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞—Ö
           for nt in available_neurotransmitters:
               percentage = percentage_scores.get(nt, 0)
               level = get_level_emoji(percentage)
               level_bar = "‚ñì" * int(percentage / 10) + "‚ñë" * (10 - int(percentage / 10))

               result_text += f"{neurotransmitters[nt]['emoji']} *{neurotransmitters[nt]['name']}*: {level}\n"
               result_text += f"{level_bar} {int(percentage)}%\n"
               result_text += f"_{neurotransmitters[nt]['description']}_\n\n"

           await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 3: –ì–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–π –ø—Ä–æ—Ñ—ñ–ª—å
       if available_hormones:
           result_text = "*–ì–û–†–ú–û–ù–ê–õ–¨–ù–ò–ô –ü–†–û–§–Ü–õ–¨:*\n\n"

           # –ü—Ä–æ—Ö–æ–¥–∏–º–æ –ø–æ –≤—Å—ñ—Ö –¥–æ—Å—Ç—É–ø–Ω–∏—Ö –≥–æ—Ä–º–æ–Ω–∞—Ö
           for nt in available_hormones:
               percentage = percentage_scores.get(nt, 0)
               level = get_level_emoji(percentage)
               level_bar = "‚ñì" * int(percentage / 10) + "‚ñë" * (10 - int(percentage / 10))

               result_text += f"{neurotransmitters[nt]['emoji']} *{neurotransmitters[nt]['name']}*: {level}\n"
               result_text += f"{level_bar} {int(percentage)}%\n"
               result_text += f"_{neurotransmitters[nt]['description']}_\n\n"

           await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 4: –°–∏—Å—Ç–µ–º–Ω–∏–π –ø—Ä–æ—Ñ—ñ–ª—å
       if available_systems:
           result_text = "*–°–ò–°–¢–ï–ú–ù–ò–ô –ü–†–û–§–Ü–õ–¨:*\n\n"

           # –ü—Ä–æ—Ö–æ–¥–∏–º–æ –ø–æ –≤—Å—ñ—Ö –¥–æ—Å—Ç—É–ø–Ω–∏—Ö —Å–∏—Å—Ç–µ–º–∞—Ö
           for nt in available_systems:
               percentage = percentage_scores.get(nt, 0)
               level = get_level_emoji(percentage)
               level_bar = "‚ñì" * int(percentage / 10) + "‚ñë" * (10 - int(percentage / 10))

               result_text += f"{neurotransmitters[nt]['emoji']} *{neurotransmitters[nt]['name']}*: {level}\n"
               result_text += f"{level_bar} {int(percentage)}%\n"
               result_text += f"_{neurotransmitters[nt]['description']}_\n\n"

           await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 5: –ö–ª—é—á–æ–≤—ñ –ø–æ–∫–∞–∑–Ω–∏–∫–∏
       result_text = "*–ö–õ–Æ–ß–û–í–Ü –ü–û–ö–ê–ó–ù–ò–ö–ò:*\n\n"

       # –î–æ–º—ñ–Ω—É—é—á–∏–π –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä
       if dominant_neurotransmitter:
           result_text += f"‚úÖ *–î–æ–º—ñ–Ω—É—é—á–∏–π –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä: {neurotransmitters[dominant_neurotransmitter]['name']}*\n"
           result_text += f"{neurotransmitters[dominant_neurotransmitter]['personality']}\n\n"

       # –î–µ—Ñ—ñ—Ü–∏—Ç–Ω–∏–π –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä
       if deficient_neurotransmitter:
           result_text += f"‚ùó *–ù–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä, —â–æ –ø–æ—Ç—Ä–µ–±—É—î –ø—ñ–¥—Ç—Ä–∏–º–∫–∏: {neurotransmitters[deficient_neurotransmitter]['name']}*\n"
           result_text += f"–ù–∏–∑—å–∫–∏–π —Ä—ñ–≤–µ–Ω—å {neurotransmitters[deficient_neurotransmitter]['deficiency']}\n\n"

       # –î–æ–º—ñ–Ω—É—é—á–∏–π –≥–æ—Ä–º–æ–Ω, —è–∫—â–æ —î
       if dominant_hormone:
           result_text += f"‚úÖ *–î–æ–º—ñ–Ω—É—é—á–∏–π –≥–æ—Ä–º–æ–Ω: {neurotransmitters[dominant_hormone]['name']}*\n"
           result_text += f"{neurotransmitters[dominant_hormone]['personality']}\n\n"

       # –î–µ—Ñ—ñ—Ü–∏—Ç–Ω–∏–π –≥–æ—Ä–º–æ–Ω, —è–∫—â–æ —î
       if deficient_hormone:
           result_text += f"‚ùó *–ì–æ—Ä–º–æ–Ω, —â–æ –ø–æ—Ç—Ä–µ–±—É—î –ø—ñ–¥—Ç—Ä–∏–º–∫–∏: {neurotransmitters[deficient_hormone]['name']}*\n"
           if 'deficiency' in neurotransmitters[deficient_hormone]:
               result_text += f"–ù–∏–∑—å–∫–∏–π —Ä—ñ–≤–µ–Ω—å {neurotransmitters[deficient_hormone]['deficiency']}\n\n"
           elif 'excess' in neurotransmitters[deficient_hormone]:
               result_text += f"–ü–æ—Ç–µ–Ω—Ü—ñ–π–Ω–∏–π –Ω–∞–¥–ª–∏—à–æ–∫ {neurotransmitters[deficient_hormone]['excess']}\n\n"

       # –î–µ—Ñ—ñ—Ü–∏—Ç–Ω–∞ —Å–∏—Å—Ç–µ–º–∞, —è–∫—â–æ —î
       if deficient_system:
           result_text += f"‚ùó *–°–∏—Å—Ç–µ–º–∞, —â–æ –ø–æ—Ç—Ä–µ–±—É—î –ø—ñ–¥—Ç—Ä–∏–º–∫–∏: {neurotransmitters[deficient_system]['name']}*\n"
           if 'deficiency' in neurotransmitters[deficient_system]:
               result_text += f"–ü—Ä–æ–±–ª–µ–º–∏ –∑ —Ü—ñ—î—é —Å–∏—Å—Ç–µ–º–æ—é {neurotransmitters[deficient_system]['deficiency']}\n\n"

       await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 6: –í–∑–∞—î–º–æ–¥—ñ—è –∑ —Å–∏—Å—Ç–µ–º–∞–º–∏
       result_text = ""

       # –î–æ–¥–∞—î–º–æ –≤–∏–ª–∫—É –≤–∑–∞—î–º–æ–¥—ñ—ó –¥–ª—è –¥–µ—Ñ—ñ—Ü–∏—Ç–Ω–æ–≥–æ –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
       if deficient_neurotransmitter and 'vylka' in neurotransmitters[deficient_neurotransmitter]:
           result_text += f"*–í–ó–ê–Ñ–ú–û–î–Ü–Ø {neurotransmitters[deficient_neurotransmitter]['name'].upper()} –ó –Ü–ù–®–ò–ú–ò –°–ò–°–¢–ï–ú–ê–ú–ò:*\n"
           result_text += f"{neurotransmitters[deficient_neurotransmitter]['vylka']}\n\n"

       # –î–æ–¥–∞—î–º–æ –≤–∏–ª–∫—É –≤–∑–∞—î–º–æ–¥—ñ—ó –¥–ª—è –¥–µ—Ñ—ñ—Ü–∏—Ç–Ω–æ–≥–æ –≥–æ—Ä–º–æ–Ω—É, —è–∫—â–æ —î
       if deficient_hormone and 'vylka' in neurotransmitters[deficient_hormone]:
           result_text += f"*–í–ó–ê–Ñ–ú–û–î–Ü–Ø {neurotransmitters[deficient_hormone]['name'].upper()} –ó –Ü–ù–®–ò–ú–ò –°–ò–°–¢–ï–ú–ê–ú–ò:*\n"
           result_text += f"{neurotransmitters[deficient_hormone]['vylka']}\n\n"

       if result_text:
           await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 7: –†–µ–∫–æ–º–µ–Ω–¥–æ–≤–∞–Ω—ñ –∞–Ω–∞–ª—ñ–∑–∏
       result_text = "*–†–ï–ö–û–ú–ï–ù–î–û–í–ê–ù–Ü –ê–ù–ê–õ–Ü–ó–ò:*\n"

       # –ë–∞–∑–æ–≤–∏–π –Ω–∞–±—ñ—Ä –∞–Ω–∞–ª—ñ–∑—ñ–≤
       basic_tests = "‚Ä¢ –ó–∞–≥–∞–ª—å–Ω–∏–π –∞–Ω–∞–ª—ñ–∑ –∫—Ä–æ–≤—ñ (–ó–ê–ö)\n"

       # –î–æ–¥–∞—Ç–∫–æ–≤—ñ –∞–Ω–∞–ª—ñ–∑–∏ –∑–∞–ª–µ–∂–Ω–æ –≤—ñ–¥ –≤–∏—è–≤–ª–µ–Ω–∏—Ö –ø—Ä–æ–±–ª–µ–º
       gender = context.user_data.get('gender', '–ù–µ –≤–∫–∞–∑–∞–Ω–æ')
       if gender == '–ß–æ–ª–æ–≤—ñ–∫':
           if deficient_hormone in ['testosterone', 'estradiol_male', 'prolactin']:
               result_text += basic_tests
               result_text += "‚Ä¢ –°—Ç–∞—Ç–µ–≤–∞ –ø–∞–Ω–µ–ª—å –¥–ª—è —á–æ–ª–æ–≤—ñ–∫—ñ–≤:\n"
               result_text += "  - –¢–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω –∑–∞–≥–∞–ª—å–Ω–∏–π —Ç–∞ –≤—ñ–ª—å–Ω–∏–π\n"
               result_text += "  - –ì–ó–°–ì (–≥–ª–æ–±—É–ª—ñ–Ω, —â–æ –∑–≤'—è–∑—É—î —Å—Ç–∞—Ç–µ–≤—ñ –≥–æ—Ä–º–æ–Ω–∏)\n"
               result_text += "  - –ü—Ä–æ–ª–∞–∫—Ç–∏–Ω\n"
               result_text += "  - –ï—Å—Ç—Ä–∞–¥—ñ–æ–ª\n"
               result_text += "  - –ü—Ä–æ–≥–µ—Å—Ç–µ—Ä–æ–Ω\n"
               result_text += "  - –õ–ì, –§–°–ì\n"
       else:  # –ñ—ñ–Ω–∫–∞
           if deficient_hormone in ['female_hormones', 'estradiol_female', 'prolactin']:
               result_text += basic_tests
               result_text += "‚Ä¢ –°—Ç–∞—Ç–µ–≤–∞ –ø–∞–Ω–µ–ª—å –¥–ª—è –∂—ñ–Ω–æ–∫:\n"
               result_text += "  - –ï—Å—Ç—Ä–∞–¥—ñ–æ–ª, –ø—Ä–æ–≥–µ—Å—Ç–µ—Ä–æ–Ω\n"
               result_text += "  - –§–°–ì, –õ–ì\n"
               result_text += "  - –ü—Ä–æ–ª–∞–∫—Ç–∏–Ω\n"
               result_text += "  - –¢–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω –∑–∞–≥–∞–ª—å–Ω–∏–π\n"
               result_text += "  - –î–ì–ï–ê-—Å—É–ª—å—Ñ–∞—Ç\n"

       # –Ø–∫—â–æ –ø—Ä–æ–±–ª–µ–º–∏ –∑ –ø–µ—á—ñ–Ω–∫–æ—é, –∫–æ—Ä—Ç–∏–∑–æ–ª–æ–º –∞–±–æ —Å–Ω–æ–º
       if any(nt in ['liver', 'cortisol', 'sleep'] for nt in available_systems):
           if 'liver' in available_systems:
               result_text += "‚Ä¢ –ü–µ—á—ñ–Ω–∫–æ–≤—ñ –ø—Ä–æ–±–∏: –ê–°–¢, –ê–õ–¢, –ë—ñ–ª—ñ—Ä—É–±—ñ–Ω\n"

           if 'cortisol' in available_systems:
               result_text += "‚Ä¢ –ö–æ—Ä—Ç–∏–∑–æ–ª (–±–∞–∂–∞–Ω–æ –∞–Ω–∞–ª—ñ–∑ —Å–ª–∏–Ω–∏ –Ω–∞ 3-4 —Ç–æ—á–∫–∏ –ø—Ä–æ—Ç—è–≥–æ–º –¥–Ω—è)\n"

           if 'sleep' in available_systems:
               result_text += "‚Ä¢ –ú–µ–ª–∞—Ç–æ–Ω—ñ–Ω\n"

       # –Ü–Ω—Å—É–ª—ñ–Ω –¥–ª—è –≤—Å—ñ—Ö
       result_text += "‚Ä¢ –Ü–Ω—Å—É–ª—ñ–Ω –Ω–∞—Ç—â–µ\n"
       result_text += "‚Ä¢ –£–ó–î –≤–Ω—É—Ç—Ä—ñ—à–Ω—ñ—Ö –æ—Ä–≥–∞–Ω—ñ–≤\n\n"

       await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 8: –†–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó
       result_text = ""

       # –ë–∞–∑–æ–≤—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –¥–ª—è –Ω–µ–π—Ä–æ–º–µ–¥—ñ–∞—Ç–æ—Ä–∞
       if deficient_neurotransmitter:
           result_text += "*–†–ï–ö–û–ú–ï–ù–î–ê–¶–Ü–á –î–õ–Ø –ù–ï–ô–†–û–ú–ï–î–Ü–ê–¢–û–†–ù–û–ì–û –ë–ê–õ–ê–ù–°–£:*\n"
           for i, recommendation in enumerate(neurotransmitters[deficient_neurotransmitter]['basic_recommendations']):
               if i < 4:  # –ü–æ–∫–∞–∑—É—î–º–æ —Ç—ñ–ª—å–∫–∏ –ø–µ—Ä—à—ñ 4 —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –≤ –±–∞–∑–æ–≤–æ–º—É –∑–≤—ñ—Ç—ñ
                   result_text += f"‚Ä¢ {recommendation}\n"

           result_text += "\n"

       # –ë–∞–∑–æ–≤—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –¥–ª—è –≥–æ—Ä–º–æ–Ω—É, —è–∫—â–æ —î
       if deficient_hormone:
           result_text += "*–†–ï–ö–û–ú–ï–ù–î–ê–¶–Ü–á –î–õ–Ø –ì–û–†–ú–û–ù–ê–õ–¨–ù–û–ì–û –ë–ê–õ–ê–ù–°–£:*\n"
           for i, recommendation in enumerate(neurotransmitters[deficient_hormone]['basic_recommendations']):
               if i < 4:  # –ü–æ–∫–∞–∑—É—î–º–æ —Ç—ñ–ª—å–∫–∏ –ø–µ—Ä—à—ñ 4 —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –≤ –±–∞–∑–æ–≤–æ–º—É –∑–≤—ñ—Ç—ñ
                   result_text += f"‚Ä¢ {recommendation}\n"

           result_text += "\n"

       # –ë–∞–∑–æ–≤—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –¥–ª—è —Å–∏—Å—Ç–µ–º–∏, —è–∫—â–æ —î
       if deficient_system:
           result_text += "*–†–ï–ö–û–ú–ï–ù–î–ê–¶–Ü–á –î–õ–Ø –°–ò–°–¢–ï–ú–ù–û–á –ü–Ü–î–¢–†–ò–ú–ö–ò:*\n"
           for i, recommendation in enumerate(neurotransmitters[deficient_system]['basic_recommendations']):
               if i < 4:  # –ü–æ–∫–∞–∑—É—î–º–æ —Ç—ñ–ª—å–∫–∏ –ø–µ—Ä—à—ñ 4 —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –≤ –±–∞–∑–æ–≤–æ–º—É –∑–≤—ñ—Ç—ñ
                   result_text += f"‚Ä¢ {recommendation}\n"

           result_text += "\n"

       if result_text:
           await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 9: –í—ñ–∫–æ–≤—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó
       age_group = context.user_data.get('age', '–ù–µ –≤–∫–∞–∑–∞–Ω–æ')

       result_text = "*–†–ï–ö–û–ú–ï–ù–î–ê–¶–Ü–á –ü–û –ú–û–ù–Ü–¢–û–†–ò–ù–ì–£ –ì–û–†–ú–û–ù–Ü–í –î–õ–Ø –í–ê–®–û–ì–û –í–Ü–ö–£:*\n\n"

       if age_group == "–î–æ 25 —Ä–æ–∫—ñ–≤":
           result_text += (
               "*–í–ê–ñ–õ–ò–í–Ü–°–¢–¨ –ú–û–ù–Ü–¢–û–†–ò–ù–ì–£ –ì–û–†–ú–û–ù–Ü–í –£ –ú–û–õ–û–î–û–ú–£ –í–Ü–¶–Ü*\n\n"
               "–ú–æ–ª–æ–¥–∏–π –≤—ñ–∫ - —ñ–¥–µ–∞–ª—å–Ω–∏–π —á–∞—Å –¥–ª—è –ø–æ—á–∞—Ç–∫—É –∫–æ–Ω—Ç—Ä–æ–ª—é –∑–∞ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–º –∑–¥–æ—Ä–æ–≤'—è–º! üí™\n\n"
               "–ù–∞–≤—ñ—Ç—å —è–∫—â–æ —Ç–∏ –ø–æ—á—É–≤–∞—î—à—Å—è —á—É–¥–æ–≤–æ, —Å–∞–º–µ –∑–∞—Ä–∞–∑ –≤–∞–∂–ª–∏–≤–æ –∑—Ä–æ–±–∏—Ç–∏ –ø–µ—Ä—à–∏–π \"–≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–π –∑–Ω—ñ–º–æ–∫\". –¶–µ–π –±–∞–∑–æ–≤–∏–π –∞–Ω–∞–ª—ñ–∑ –¥–æ–∑–≤–æ–ª–∏—Ç—å:\n"
               "‚Ä¢ –í—Å—Ç–∞–Ω–æ–≤–∏—Ç–∏ —Ç–≤–æ—ó –æ—Å–æ–±–∏—Å—Ç—ñ –Ω–æ—Ä–º–∏ –¥–ª—è –ø–æ–¥–∞–ª—å—à–æ–≥–æ –ø–æ—Ä—ñ–≤–Ω—è–Ω–Ω—è\n"
               "‚Ä¢ –í–∏—è–≤–∏—Ç–∏ –ø—Ä–∏—Ö–æ–≤–∞–Ω—ñ –¥–∏—Å–±–∞–ª–∞–Ω—Å–∏ –Ω–∞ —Ä–∞–Ω–Ω—ñ–π —Å—Ç–∞–¥—ñ—ó\n"
               "‚Ä¢ –°—Ç–≤–æ—Ä–∏—Ç–∏ –ø—Ä–∞–≤–∏–ª—å–Ω—É —Å—Ç—Ä–∞—Ç–µ–≥—ñ—é –¥–ª—è –¥–æ—Å—è–≥–Ω–µ–Ω–Ω—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–∏—Ö —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ —É —Å–ø–æ—Ä—Ç—ñ\n\n"
               "–¢–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω –Ω–∞–ø—Ä—è–º—É –≤–ø–ª–∏–≤–∞—î –Ω–∞ —Ç–≤–æ—é –∑–¥–∞—Ç–Ω—ñ—Å—Ç—å –Ω–∞–±–∏—Ä–∞—Ç–∏ –º'—è–∑–æ–≤—É –º–∞—Å—É, –ø—ñ–¥—Ç—Ä–∏–º—É–≤–∞—Ç–∏ –Ω–∏–∑—å–∫–∏–π –≤—ñ–¥—Å–æ—Ç–æ–∫ –∂–∏—Ä—É, "
               "–∑–±–µ—Ä—ñ–≥–∞—Ç–∏ –≤–∏—Å–æ–∫—É –µ–Ω–µ—Ä–≥—ñ–π–Ω—ñ—Å—Ç—å —Ç–∞ —è—Å–Ω—ñ—Å—Ç—å –º–∏—Å–ª–µ–Ω–Ω—è.\n\n"
               "–Ü–Ω–≤–µ—Å—Ç—É–π —É —Å–≤–æ—î –∑–¥–æ—Ä–æ–≤'—è –∑–∞—Ä–∞–∑ - —ñ –æ—Ç—Ä–∏–º—É–π –¥–∏–≤—ñ–¥–µ–Ω–¥–∏ –≤—Å–µ –∂–∏—Ç—Ç—è!"
           )
       elif age_group == "25-35 —Ä–æ–∫—ñ–≤":
           result_text += (
               "*–ö–û–ù–¢–†–û–õ–¨ –ì–û–†–ú–û–ù–Ü–í –£ –í–Ü–¶–Ü 25-35 –†–û–ö–Ü–í - –ö–õ–Æ–ß –î–û –î–û–í–ì–û–°–¢–†–û–ö–û–í–û–ì–û –ó–î–û–†–û–í'–Ø*\n\n"
               "–£ —Ü—å–æ–º—É –≤—ñ—Ü—ñ —Ç—ñ–ª–æ –≤–∂–µ –Ω–µ —Ç–∞–∫–µ \"–≤—Å–µ–ø—Ä–æ—â–∞—é—á–µ\", —è–∫ —É 20 —Ä–æ–∫—ñ–≤! üïí\n\n"
               "–°–∞–º–µ –∑–∞—Ä–∞–∑ –≤—ñ–¥–±—É–≤–∞—é—Ç—å—Å—è –ø–µ—Ä—à—ñ –∑–Ω–∞—á—É—â—ñ –∑–º—ñ–Ω–∏ –≤ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–º—É –ø—Ä–æ—Ñ—ñ–ª—ñ, —è–∫—ñ –º–æ–∂—É—Ç—å –≤–ø–ª–∏–≤–∞—Ç–∏ –Ω–∞:\n"
               "‚Ä¢ –ï–Ω–µ—Ä–≥—ñ–π–Ω—ñ—Å—Ç—å –ø—Ä–æ—Ç—è–≥–æ–º –¥–Ω—è\n"
               "‚Ä¢ –í—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è –ø—ñ—Å–ª—è –Ω–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω—å\n"
               "‚Ä¢ –ï–º–æ—Ü—ñ–π–Ω—É —Å—Ç–∞–±—ñ–ª—å–Ω—ñ—Å—Ç—å —Ç–∞ —Å—Ç—Ä–µ—Å–æ—Å—Ç—ñ–π–∫—ñ—Å—Ç—å\n"
               "‚Ä¢ –Ø–∫—ñ—Å—Ç—å —Å–Ω—É\n\n"
               "–†–µ–≥—É–ª—è—Ä–Ω–∏–π –º–æ–Ω—ñ—Ç–æ—Ä–∏–Ω–≥ –≥–æ—Ä–º–æ–Ω—ñ–≤ –¥–æ–∑–≤–æ–ª–∏—Ç—å —Ç–æ–±—ñ –≤—ñ–¥—Å—Ç–µ–∂—É–≤–∞—Ç–∏ –¥–∏–Ω–∞–º—ñ–∫—É –∑–º—ñ–Ω —ñ –≤—á–∞—Å–Ω–æ —Ä–µ–∞–≥—É–≤–∞—Ç–∏, "
               "–Ω–µ –¥–æ–ø—É—Å–∫–∞—é—á–∏ –∑–Ω–∏–∂–µ–Ω–Ω—è —è–∫–æ—Å—Ç—ñ –∂–∏—Ç—Ç—è —Ç–∞ –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–æ—Å—Ç—ñ.\n\n"
               "–ù–µ —á–µ–∫–∞–π –æ—á–µ–≤–∏–¥–Ω–∏—Ö —Å–∏–º–ø—Ç–æ–º—ñ–≤ - –¥—ñ–π –Ω–∞ –≤–∏–ø–µ—Ä–µ–¥–∂–µ–Ω–Ω—è!"
           )
       elif age_group in ["36-45 —Ä–æ–∫—ñ–≤", "46-60 —Ä–æ–∫—ñ–≤"]:
           result_text += (
               "*–ì–û–†–ú–û–ù–ê–õ–¨–ù–ò–ô –ë–ê–õ–ê–ù–° –£ –ó–†–Ü–õ–û–ú–£ –í–Ü–¶–Ü - –¢–í–û–Ø –î–†–£–ì–ê –ú–û–õ–û–î–Ü–°–¢–¨*\n\n"
               "–¶–µ–π –≤—ñ–∫ - —á–∞—Å –∫–æ–ª–∏ –∫–æ–Ω—Ç—Ä–æ–ª—å –Ω–∞–¥ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–º –±–∞–ª–∞–Ω—Å–æ–º —Å—Ç–∞—î –∫—Ä–∏—Ç–∏—á–Ω–æ –≤–∞–∂–ª–∏–≤–∏–º! ‚öñÔ∏è\n\n"
               "–ó–∞ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–æ—é, —Å–∞–º–µ –≤ —Ü—å–æ–º—É –≤—ñ–∫–æ–≤–æ–º—É –ø—Ä–æ–º—ñ–∂–∫—É –≤—ñ–¥–±—É–≤–∞—î—Ç—å—Å—è –Ω–∞–π–±—ñ–ª—å—à –≤—ñ–¥—á—É—Ç–Ω–µ –∑–Ω–∏–∂–µ–Ω–Ω—è —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É "
               "—É —á–æ–ª–æ–≤—ñ–∫—ñ–≤ (–¥–æ 1-2% –Ω–∞ —Ä—ñ–∫). –°–≤–æ—î—á–∞—Å–Ω–∞ –¥—ñ–∞–≥–Ω–æ—Å—Ç–∏–∫–∞ –¥–æ–∑–≤–æ–ª–∏—Ç—å:\n"
               "‚Ä¢ –ü—ñ–¥—Ç—Ä–∏–º—É–≤–∞—Ç–∏ –≤–∏—Å–æ–∫—É –∂–∏—Ç—Ç—î–≤—É –µ–Ω–µ—Ä–≥—ñ—é\n"
               "‚Ä¢ –ó–±–µ—Ä–µ–≥—Ç–∏ –º'—è–∑–æ–≤—É –º–∞—Å—É, —è–∫–∞ –ø–æ—á–∏–Ω–∞—î –ø—Ä–∏—Ä–æ–¥–Ω–æ –∑–Ω–∏–∂—É–≤–∞—Ç–∏—Å—å\n"
               "‚Ä¢ –ö–æ–Ω—Ç—Ä–æ–ª—é–≤–∞—Ç–∏ –≤—ñ—Å—Ü–µ—Ä–∞–ª—å–Ω–∏–π –∂–∏—Ä (–Ω–∞–≤–∫–æ–ª–æ –≤–Ω—É—Ç—Ä—ñ—à–Ω—ñ—Ö –æ—Ä–≥–∞–Ω—ñ–≤)\n"
               "‚Ä¢ –ü—ñ–¥—Ç—Ä–∏–º—É–≤–∞—Ç–∏ –ª—ñ–±—ñ–¥–æ —Ç–∞ —Å–µ–∫—Å—É–∞–ª—å–Ω—É —Ñ—É–Ω–∫—Ü—ñ—é\n"
               "‚Ä¢ –ó–∞–ø–æ–±—ñ–≥–∞—Ç–∏ –ø–æ–≥—ñ—Ä—à–µ–Ω–Ω—é –∫–æ–≥–Ω—ñ—Ç–∏–≤–Ω–∏—Ö —Ñ—É–Ω–∫—Ü—ñ–π\n\n"
               "–©–æ—Ä—ñ—á–Ω–µ –≤—ñ–¥—Å—Ç–µ–∂–µ–Ω–Ω—è —Ä—ñ–≤–Ω—è —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É, –µ—Å—Ç—Ä–∞–¥—ñ–æ–ª—É —Ç–∞ —ñ–Ω—à–∏—Ö –ø–æ–∫–∞–∑–Ω–∏–∫—ñ–≤ - —Ü–µ –Ω–µ —Ä–æ–∑–∫—ñ—à, –∞ –Ω–µ–æ–±—Ö—ñ–¥–Ω—ñ—Å—Ç—å –¥–ª—è "
               "—Å—É—á–∞—Å–Ω–æ–≥–æ —á–æ–ª–æ–≤—ñ–∫–∞, —è–∫–∏–π —Ö–æ—á–µ –∑–∞–ª–∏—à–∞—Ç–∏—Å—è –≤ —Ñ–æ—Ä–º—ñ —Ç–∞ –∑–±–µ—Ä—ñ–≥–∞—Ç–∏ –≤–∏—Å–æ–∫—É —è–∫—ñ—Å—Ç—å –∂–∏—Ç—Ç—è."
           )
       elif age_group == "60+ —Ä–æ–∫—ñ–≤":
           result_text += (
               "*–ì–û–†–ú–û–ù–ê–õ–¨–ù–ò–ô –ë–ê–õ–ê–ù–° –ü–Ü–°–õ–Ø 60 –†–û–ö–Ü–í - –û–°–ù–û–í–ê –Ø–ö–Ü–°–ù–û–ì–û –î–û–í–ì–û–õ–Ü–¢–¢–Ø*\n\n"
               "–£ —Ü—å–æ–º—É –ø–æ–≤–∞–∂–Ω–æ–º—É –≤—ñ—Ü—ñ —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω - —Ü–µ –Ω–µ –ø—Ä–æ—Å—Ç–æ –≥–æ—Ä–º–æ–Ω, –∞ –∫–ª—é—á –¥–æ –ø—Ä–æ–¥–æ–≤–∂–µ–Ω–Ω—è –∞–∫—Ç–∏–≤–Ω–æ–≥–æ —ñ –ø–æ–≤–Ω–æ—Ü—ñ–Ω–Ω–æ–≥–æ –∂–∏—Ç—Ç—è! üï∞Ô∏è\n\n"
               "–ü—ñ—Å–ª—è 60 —Ä–æ–∫—ñ–≤ –ø—Ä–∏—Ä–æ–¥–Ω–µ –∑–Ω–∏–∂–µ–Ω–Ω—è —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É –º–æ–∂–µ —Å—É—Ç—Ç—î–≤–æ –ø—Ä–∏—Å–∫–æ—Ä—é–≤–∞—Ç–∏—Å—å, —â–æ –ø—Ä—è–º–æ –≤–ø–ª–∏–≤–∞—î –Ω–∞:\n"
               "‚Ä¢ –ó–∞–≥–∞–ª—å–Ω—É –∂–∏—Ç—Ç—î–≤—É –µ–Ω–µ—Ä–≥—ñ—é —Ç–∞ —Å–∞–º–æ–ø–æ—á—É—Ç—Ç—è\n"
               "‚Ä¢ –ü—ñ–¥—Ç—Ä–∏–º–∫—É –º'—è–∑–æ–≤–æ—ó –º–∞—Å–∏ —Ç–∞ —Å–∏–ª–∏ (–∫—Ä–∏—Ç–∏—á–Ω–æ –≤–∞–∂–ª–∏–≤–æ –¥–ª—è —Å–∞–º–æ—Å—Ç—ñ–π–Ω–æ—Å—Ç—ñ)\n"
               "‚Ä¢ –©—ñ–ª—å–Ω—ñ—Å—Ç—å –∫—ñ—Å—Ç–∫–æ–≤–æ—ó —Ç–∫–∞–Ω–∏–Ω–∏ (–ø—Ä–æ—Ñ—ñ–ª–∞–∫—Ç–∏–∫–∞ –æ—Å—Ç–µ–æ–ø–æ—Ä–æ–∑—É)\n"
               "‚Ä¢ –ö–æ–≥–Ω—ñ—Ç–∏–≤–Ω—ñ —Ñ—É–Ω–∫—Ü—ñ—ó —Ç–∞ —è—Å–Ω—ñ—Å—Ç—å –º–∏—Å–ª–µ–Ω–Ω—è\n"
               "‚Ä¢ –°–µ—Ä—Ü–µ–≤–æ-—Å—É–¥–∏–Ω–Ω–µ –∑–¥–æ—Ä–æ–≤'—è\n\n"
               "–†–µ–≥—É–ª—è—Ä–Ω–∏–π –º–æ–Ω—ñ—Ç–æ—Ä–∏–Ω–≥ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ —Ñ–æ–Ω—É –¥–æ–∑–≤–æ–ª—è—î —Å–≤–æ—î—á–∞—Å–Ω–æ –≤–∏–∑–Ω–∞—á–∏—Ç–∏ –Ω–µ–æ–±—Ö—ñ–¥–Ω—ñ—Å—Ç—å –∑–∞–º—ñ—Å–Ω–æ—ó –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ—ó —Ç–µ—Ä–∞–ø—ñ—ó (–ì–ó–¢) "
               "–∞–±–æ —ñ–Ω—à–∏—Ö –º–µ—Ç–æ–¥—ñ–≤ –ø—ñ–¥—Ç—Ä–∏–º–∫–∏.\n\n"
               "–ß–∞—Å –ø–µ—Ä–µ–ø–∏—Å–∞—Ç–∏ –ø—Ä–∞–≤–∏–ª–∞ —Å—Ç–∞—Ä—ñ–Ω–Ω—è —ñ –∑–±–µ—Ä–µ–≥—Ç–∏ –∞–∫—Ç–∏–≤–Ω—ñ—Å—Ç—å, –µ–Ω–µ—Ä–≥—ñ—é —Ç–∞ —Ä–∞–¥—ñ—Å—Ç—å –∂–∏—Ç—Ç—è –Ω–∞ –¥–æ–≤–≥—ñ —Ä–æ–∫–∏!"
           )

       await update.message.reply_text(result_text, parse_mode='Markdown')

       # –ü–æ–≤—ñ–¥–æ–º–ª–µ–Ω–Ω—è 10: –§—ñ–Ω–∞–ª—å–Ω—ñ –ø—Ä–æ–ø–æ–∑–∏—Ü—ñ—ó —Ç–∞ –∫–Ω–æ–ø–∫–∏
       result_text = "*–©–û –î–ê–õ–Ü?*\n"
       result_text += "–ù–∞ –æ—Å–Ω–æ–≤—ñ –≤–∞—à–æ–≥–æ –≤—ñ–∫—É —Ç–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ —Ç–µ—Å—Ç—É –º–∏ –ø—ñ–¥–≥–æ—Ç—É–≤–∞–ª–∏ —Å–ø–µ—Ü—ñ–∞–ª—å–Ω—ñ –ø—Ä–æ–ø–æ–∑–∏—Ü—ñ—ó:\n\n"

       # –†—ñ–∑–Ω—ñ –ø—Ä–æ–ø–æ–∑–∏—Ü—ñ—ó –∑–∞–ª–µ–∂–Ω–æ –≤—ñ–¥ –≤—ñ–∫—É
       if age_group == "–î–æ 25 —Ä–æ–∫—ñ–≤":
           result_text += (
               "üîπ *–†–æ–∑—à–∏—Ä–µ–Ω–∏–π –∞–Ω–∞–ª—ñ–∑ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é* ‚Äî –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è –¥–ª—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–≥–æ —Ä–æ—Å—Ç—É –º'—è–∑—ñ–≤\n"
               "üîπ *–ü—Ä–æ–≥—Ä–∞–º–∞ —Ç—Ä–µ–Ω—É–≤–∞–Ω—å –¥–ª—è –Ω–∞–±–æ—Ä—É –º'—è–∑–æ–≤–æ—ó –º–∞—Å–∏* ‚Äî —Å–ø–µ—Ü—ñ–∞–ª—å–Ω–æ –¥–ª—è –≤–∞—à–æ–≥–æ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é\n"
               "üîπ *–ì–∞–π–¥ –Ω–∞ –ø–æ—Å–∏–ª–µ–Ω–Ω—è –ª—ñ–±—ñ–¥–æ* ‚Äî –∑–∞—Å–æ–±–∏ –¥–ª—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ—ó —Å–µ–∫—Å—É–∞–ª—å–Ω–æ—ó –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–æ—Å—Ç—ñ\n"
               "üîπ *–ì–∞–π–¥ –ø–æ —Ä–æ—Å—Ç—É –±–æ—Ä–æ–¥–∏* ‚Äî —è–∫ —Å—Ç–∏–º—É–ª—é–≤–∞—Ç–∏ —Ä—ñ—Å—Ç –≥—É—Å—Ç–æ—ó —ñ —Ä—ñ–≤–Ω–æ—ó –±–æ—Ä–æ–¥–∏"
           )
       elif age_group == "25-35 —Ä–æ–∫—ñ–≤":
           result_text += (
               "üîπ *–†–æ–∑—à–∏—Ä–µ–Ω–∏–π –∑–≤—ñ—Ç* ‚Äî –¥–µ—Ç–∞–ª—å–Ω—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –¥–ª—è –ø—ñ–¥—Ç—Ä–∏–º–∫–∏ –ø—Ä–∏—Ä–æ–¥–Ω–æ–≥–æ –ø—ñ–∫—É —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É\n"
               "üîπ *–ü—Ä–æ–≥—Ä–∞–º–∞ —Ç—Ä–µ–Ω—É–≤–∞–Ω—å –∑ —É—Ä–∞—Ö—É–≤–∞–Ω–Ω—è–º –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏—Ö –æ—Å–æ–±–ª–∏–≤–æ—Å—Ç–µ–π* ‚Äî –º–∞–∫—Å–∏–º–∞–ª—å–Ω–∞ –µ—Ñ–µ–∫—Ç–∏–≤–Ω—ñ—Å—Ç—å\n"
               "üîπ *–ì–∞–π–¥ –Ω–∞ –ø–æ—Å–∏–ª–µ–Ω–Ω—è –ª—ñ–±—ñ–¥–æ* ‚Äî –∑–∞—Å–æ–±–∏ –¥–ª—è –≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è —Ç–∞ –ø—ñ–¥—Ç—Ä–∏–º–∫–∏ —Å–µ–∫—Å—É–∞–ª—å–Ω–æ—ó —Ñ—É–Ω–∫—Ü—ñ—ó\n"
               "üîπ *–ì–∞–π–¥ –ø–æ –∑—É–ø–∏–Ω—Ü—ñ –≤–∏–ø–∞–¥—ñ–Ω–Ω—è –≤–æ–ª–æ—Å—Å—è* ‚Äî —è–∫ –∑–±–µ—Ä–µ–≥—Ç–∏ –∑–¥–æ—Ä–æ–≤–µ –≤–æ–ª–æ—Å—Å—è —É –Ω–∞–π–∞–∫—Ç–∏–≤–Ω—ñ—à–∏–π –ø–µ—Ä—ñ–æ–¥ –∂–∏—Ç—Ç—è"
           )
       elif age_group in ["36-45 —Ä–æ–∫—ñ–≤", "46-60 —Ä–æ–∫—ñ–≤"]:
           result_text += (
               "üîπ *–ü–æ–≤–Ω–∏–π –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–π –∞—É–¥–∏—Ç* ‚Äî –≤–∏—è–≤–ª–µ–Ω–Ω—è —Ç–∞ —É—Å—É–Ω–µ–Ω–Ω—è –≤—ñ–∫–æ–≤–∏—Ö –∑–º—ñ–Ω\n"
               "üîπ *–ü—Ä–æ–≥—Ä–∞–º–∞ –¥–ª—è –ø—ñ–¥—Ç—Ä–∏–º–∫–∏ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –∑–¥–æ—Ä–æ–≤'—è –ø—ñ—Å–ª—è 35* ‚Äî –∑—É–ø–∏–Ω—ñ—Ç—å –≤—ñ–∫–æ–≤–µ –∑–Ω–∏–∂–µ–Ω–Ω—è —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É\n"
               "üîπ *–ì–∞–π–¥ –ø–æ –∑—É–ø–∏–Ω—Ü—ñ —Å–∏–≤–∏–Ω–∏* ‚Äî –ø—Ä–∏—Ä–æ–¥–Ω—ñ –º–µ—Ç–æ–¥–∏ –±–æ—Ä–æ—Ç—å–±–∏ –∑ —Ä–∞–Ω–Ω—å–æ—é —Å–∏–≤–∏–Ω–æ—é\n"
               "üîπ *–ì–∞–π–¥ –∑ –≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è —Å–µ–∫—Å—É–∞–ª—å–Ω–æ—ó —Ñ—É–Ω–∫—Ü—ñ—ó* ‚Äî –ø–æ–≤–µ—Ä–Ω–µ–Ω–Ω—è –µ–Ω–µ—Ä–≥—ñ—ó —Ç–∞ –±–∞–∂–∞–Ω–Ω—è"
           )
       elif age_group == "60+ —Ä–æ–∫—ñ–≤":
           result_text += (
               "üîπ *–ü—Ä–æ–≥—Ä–∞–º–∞ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ—ó –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—ó 60+* ‚Äî —è–∫—ñ—Å–Ω–µ –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è\n"
               "üîπ *–ê–¥–∞–ø—Ç–æ–≤–∞–Ω—ñ —Ñ—ñ–∑–∏—á–Ω—ñ –Ω–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è* ‚Äî –±–µ–∑–ø–µ—á–Ω—ñ —Ç—Ä–µ–Ω—É–≤–∞–Ω–Ω—è –¥–ª—è –ø—ñ–¥—Ç—Ä–∏–º–∫–∏ —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω—É\n"
               "üîπ *–ö–æ–º–ø–ª–µ–∫—Å–Ω–∏–π –≥–∞–π–¥ –∑ –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Å–µ–∫—Å—É–∞–ª—å–Ω–æ–≥–æ –∑–¥–æ—Ä–æ–≤'—è 60+* ‚Äî –≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è —Ç–∞ –ø—ñ–¥—Ç—Ä–∏–º–∫–∞\n"
               "üîπ *–ì–∞–π–¥ –∑ –¥–æ–≥–ª—è–¥—É –∑–∞ –≤–æ–ª–æ—Å—Å—è–º —Ç–∞ —à–∫—ñ—Ä–æ—é* ‚Äî –¥–ª—è –∑–±–µ—Ä–µ–∂–µ–Ω–Ω—è –º–æ–ª–æ–¥–æ—Å—Ç—ñ –∑–æ–≤–Ω—ñ—à–Ω—å–æ–≥–æ –≤–∏–≥–ª—è–¥—É"
           )

       # –ö–Ω–æ–ø–∫–∏ –¥–ª—è –≤–∏–±–æ—Ä—É - –æ–Ω–æ–≤–ª–µ–Ω—ñ –∑ –Ω–æ–≤–∏–º–∏ –ø—Ä–æ–¥—É–∫—Ç–∞–º–∏
       keyboard = [
           [InlineKeyboardButton("üìä –†–æ–∑—à–∏—Ä–µ–Ω–∏–π –∑–≤—ñ—Ç", callback_data="get_extended")],
           [InlineKeyboardButton("üèãÔ∏è‚Äç‚ôÇÔ∏è –ü—Ä–æ–≥—Ä–∞–º–∞ —Ç—Ä–µ–Ω—É–≤–∞–Ω—å", callback_data="training_program")],
           [InlineKeyboardButton("‚ù§Ô∏è –ì–∞–π–¥ –Ω–∞ –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è –ª—ñ–±—ñ–¥–æ", callback_data="libido_guide")],
           [InlineKeyboardButton("üíá‚Äç‚ôÇÔ∏è –ì–∞–π–¥ –ø–æ –≤–æ–ª–æ—Å—Å—é/–±–æ—Ä–æ–¥—ñ", callback_data="hair_guide")],
           [InlineKeyboardButton("üë®‚Äç‚öïÔ∏è –ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—è —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–∞", url="https://t.me/vladyslav_kravchukk")]
       ]
       reply_markup = InlineKeyboardMarkup(keyboard)

       # –í—ñ–¥–ø—Ä–∞–≤–∫–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤
       await update.message.reply_text(result_text, parse_mode='Markdown', reply_markup=reply_markup)

       return ConversationHandler.END
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ show_results: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—ñ–¥ —á–∞—Å –≤—ñ–¥–æ–±—Ä–∞–∂–µ–Ω–Ω—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤. –°–ø—Ä–æ–±—É–π—Ç–µ —â–µ —Ä–∞–∑ /start")
       return ConversationHandler.END

# –ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—è —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–∞
async def handle_consult(update: Update, context: ContextTypes.DEFAULT_TYPE) -> int:
   try:
       await update.message.reply_text(
           "üë®‚Äç‚öïÔ∏è *–ö–û–ù–°–£–õ–¨–¢–ê–¶–Ü–Ø –°–ü–ï–¶–Ü–ê–õ–Ü–°–¢–ê* üë®‚Äç‚öïÔ∏è\n\n"
           "–î–ª—è –æ—Ç—Ä–∏–º–∞–Ω–Ω—è –ø–µ—Ä—Å–æ–Ω–∞–ª—å–Ω–æ—ó –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—ó —Ç–∞ —Ä–æ–∑—Ä–æ–±–∫–∏ —ñ–Ω–¥–∏–≤—ñ–¥—É–∞–ª—å–Ω–æ–≥–æ –ø–ª–∞–Ω—É –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—ó –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –±–∞–ª–∞–Ω—Å—É, "
           "–∑–≤–µ—Ä—Ç–∞–π—Ç–µ—Å—è –¥–æ –Ω–∞—à–æ–≥–æ –µ–∫—Å–ø–µ—Ä—Ç–∞:\n\n"
           "–í–ª–∞–¥–∏—Å–ª–∞–≤ –ö—Ä–∞–≤—á—É–∫ - —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç –∑ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ—ó –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—ó\n"
           "Telegram: @vladyslav_kravchukk\n\n"
           "–ü—ñ–¥ —á–∞—Å –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—ó –≤–∏ –æ—Ç—Ä–∏–º–∞—î—Ç–µ:\n"
           "‚Ä¢ –î–µ—Ç–∞–ª—å–Ω–∏–π –∞–Ω–∞–ª—ñ–∑ –≤–∞—à–æ–≥–æ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø—Ä–æ—Ñ—ñ–ª—é\n"
           "‚Ä¢ –Ü–Ω–¥–∏–≤—ñ–¥—É–∞–ª—å–Ω—ñ —Ä–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó —â–æ–¥–æ –¥—ñ—î—Ç–∏ —Ç–∞ —Å–ø–æ—Å–æ–±—É –∂–∏—Ç—Ç—è\n"
           "‚Ä¢ –û–ø—Ç–∏–º–∞–ª—å–Ω–∏–π –ø–ª–∞–Ω –¥–æ–¥–∞—Ç–∫–æ–≤–∏—Ö –æ–±—Å—Ç–µ–∂–µ–Ω—å (–∑–∞ –ø–æ—Ç—Ä–µ–±–∏)\n"
           "‚Ä¢ –ü–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω—ñ –ø—Ä–æ—Ç–æ–∫–æ–ª–∏ –∑–∞—Å—Ç–æ—Å—É–≤–∞–Ω–Ω—è –¥–æ–±–∞–≤–æ–∫\n\n"
           "–ü–æ–≤–µ—Ä—Ç–∞–π—Ç–µ—Å—å –¥–æ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –º–µ–Ω—é —á–µ—Ä–µ–∑ –∫–æ–º–∞–Ω–¥—É /start",
           parse_mode='Markdown'
       )
       return ConversationHandler.END
   except Exception as e:
       logger.error(f"–ü–æ–º–∏–ª–∫–∞ –≤ handle_consult: {e}", exc_info=True)
       await update.message.reply_text("–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞. –°–ø—Ä–æ–±—É–π—Ç–µ –∑–Ω–æ–≤—É /start")
       return ConversationHandler.END

# –§—É–Ω–∫—Ü—ñ—è –¥–ª—è –æ–±—Ä–æ–±–∫–∏ –Ω–∞—Ç–∏—Å–∫–∞–Ω—å –Ω–∞ –∫–Ω–æ–ø–∫–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤
# –§—É–Ω–∫—Ü—ñ—è –¥–ª—è –æ–±—Ä–æ–±–∫–∏ –Ω–∞—Ç–∏—Å–∫–∞–Ω—å –Ω–∞ –∫–Ω–æ–ø–∫–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤
async def handle_result_buttons(update: Update, context: ContextTypes.DEFAULT_TYPE):
   try:
       query = update.callback_query
       await query.answer()

       age_group = context.user_data.get('age', '–ù–µ –≤–∫–∞–∑–∞–Ω–æ')
       gender = context.user_data.get('gender', '–ù–µ –≤–∫–∞–∑–∞–Ω–æ')

       if query.data == "get_extended":
           # –§–æ—Ä–º—É—î–º–æ —Ä–æ–∑—à–∏—Ä–µ–Ω–∏–π –∑–≤—ñ—Ç –∑–∞–ª–µ–∂–Ω–æ –≤—ñ–¥ —Å—Ç–∞—Ç—ñ
           if gender == "–ß–æ–ª–æ–≤—ñ–∫":
               extended_text = (
                   "*–†–û–ó–®–ò–†–ï–ù–ò–ô –ó–í–Ü–¢ –ì–û–†–ú–û–ù–ê–õ–¨–ù–û–ì–û –ó–î–û–†–û–í'–Ø –î–õ–Ø –ß–û–õ–û–í–Ü–ö–Ü–í*\n\n"
                   "*–†–ï–ö–û–ú–ï–ù–î–û–í–ê–ù–Ü –ê–ù–ê–õ–Ü–ó–ò:*\n\n"
                   "1Ô∏è‚É£ *–¢–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω–æ–≤–∞ –ø–∞–Ω–µ–ª—å:*\n"
                   "‚Ä¢ –ó–∞–≥–∞–ª—å–Ω–∏–π —Ç–∞ –≤—ñ–ª—å–Ω–∏–π —Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω\n"
                   "‚Ä¢ –ì–ó–°–ì (–≥–ª–æ–±—É–ª—ñ–Ω, —â–æ –∑–≤'—è–∑—É—î —Å—Ç–∞—Ç–µ–≤—ñ –≥–æ—Ä–º–æ–Ω–∏)\n"
                   "‚Ä¢ –ü—Ä–æ–ª–∞–∫—Ç–∏–Ω\n"
                   "‚Ä¢ –ï—Å—Ç—Ä–∞–¥—ñ–æ–ª\n"
                   "‚Ä¢ –ü—Ä–æ–≥–µ—Å—Ç–µ—Ä–æ–Ω\n"
                   "‚Ä¢ –õ–ì, –§–°–ì\n\n"
                   "2Ô∏è‚É£ *–ó–∞–≥–∞–ª—å–Ω–∏–π –∞–Ω–∞–ª—ñ–∑ –∫—Ä–æ–≤—ñ (–ó–ê–ö):*\n"
                   "‚Ä¢ –ë–∞–∑–æ–≤–∞ –æ—Ü—ñ–Ω–∫–∞ –∑–∞–≥–∞–ª—å–Ω–æ–≥–æ —Å—Ç–∞–Ω—É –æ—Ä–≥–∞–Ω—ñ–∑–º—É\n"
                   "‚Ä¢ –í–∏—è–≤–ª–µ–Ω–Ω—è –∑–∞–ø–∞–ª–µ–Ω–Ω—è, –∞–Ω–µ–º—ñ—ó\n\n"
                   "3Ô∏è‚É£ *–©–∏—Ç–æ–≤–∏–¥–Ω–∞ –∑–∞–ª–æ–∑–∞:*\n"
                   "‚Ä¢ TSH, T3, T4, –∞–Ω—Ç–∏—Ç—ñ–ª–∞\n"
                   "‚Ä¢ –ö—Ä–∏—Ç–∏—á–Ω–æ –≤–∞–∂–ª–∏–≤–æ –¥–ª—è –µ–Ω–µ—Ä–≥—ñ—ó —Ç–∞ –º–µ—Ç–∞–±–æ–ª—ñ–∑–º—É\n\n"
                   "4Ô∏è‚É£ *–ü–µ—á—ñ–Ω–∫–æ–≤—ñ –ø—Ä–æ–±–∏:*\n"
                   "‚Ä¢ –ê–õ–¢, –ê–°–¢, –ì–ì–¢, –±—ñ–ª—ñ—Ä—É–±—ñ–Ω\n"
                   "‚Ä¢ –û—Ü—ñ–Ω–∫–∞ –¥–µ—Ç–æ–∫—Å–∏–∫–∞—Ü—ñ—ó —Ç–∞ –º–µ—Ç–∞–±–æ–ª—ñ–∑–º—É –≥–æ—Ä–º–æ–Ω—ñ–≤\n\n"
                   "5Ô∏è‚É£ *–î–æ–¥–∞—Ç–∫–æ–≤—ñ –∞–Ω–∞–ª—ñ–∑–∏:*\n"
                   "‚Ä¢ –õ—ñ–ø—ñ–¥–æ–≥—Ä–∞–º–∞ (—Ö–æ–ª–µ—Å—Ç–µ—Ä–∏–Ω, –õ–ü–ù–©, –õ–ü–í–©, —Ç—Ä–∏–≥–ª—ñ—Ü–µ—Ä–∏–¥–∏)\n"
                   "‚Ä¢ –ì–æ–º–æ—Ü–∏—Å—Ç–µ—ó–Ω - –º–∞—Ä–∫–µ—Ä –∑–∞–ø–∞–ª–µ–Ω–Ω—è\n"
                   "‚Ä¢ –í—ñ—Ç–∞–º—ñ–Ω D - 25(OH)D\n"
                   "‚Ä¢ –ö–æ—Ä—Ç–∏–∑–æ–ª (–±–∞–∂–∞–Ω–æ –≤ —Å–ª–∏–Ω—ñ 4 —Ä–∞–∑–∏ –Ω–∞ –¥–µ–Ω—å)\n\n"
                   "*–†–ï–ö–û–ú–ï–ù–î–ê–¶–Ü–á –ü–†–ò –ü–û–ù–ò–ñ–ï–ù–û–ú–£ –¢–ï–°–¢–û–°–¢–ï–†–û–ù–Ü:*\n\n"
                   "1Ô∏è‚É£ *–§—ñ–∑—ñ–æ–ª–æ–≥—ñ—è —ñ –æ–¥—è–≥:*\n"
                   "‚Ä¢ –£–Ω–∏–∫–∞–π—Ç–µ —Ç—ñ—Å–Ω–æ—ó –Ω–∏–∂–Ω—å–æ—ó –±—ñ–ª–∏–∑–Ω–∏ - –ø–æ—Ä—É—à—É—î —Ç–µ—Ä–º–æ—Ä–µ–≥—É–ª—è—Ü—ñ—é\n"
                   "‚Ä¢ –û–±–∏—Ä–∞–π—Ç–µ –∞–Ω–∞—Ç–æ–º—ñ—á–Ω—É –±—ñ–ª–∏–∑–Ω—É –∞–±–æ –≤—ñ–ª—å–Ω—ñ —Å—ñ–º–µ–π–Ω—ñ —Ç—Ä—É—Å–∏\n"
                   "‚Ä¢ –£–Ω–∏–∫–∞–π—Ç–µ —Ç—Ä–∏–≤–∞–ª–æ–≥–æ —Å–∏–¥—ñ–Ω–Ω—è - —Ä–æ–±—ñ—Ç—å –ø–µ—Ä–µ—Ä–≤–∏ –∫–æ–∂–Ω—É –≥–æ–¥–∏–Ω—É\n\n"
                   "2Ô∏è‚É£ *–ü–æ—Å—Ç–∞–≤–∞ —ñ —Ñ—ñ–∑–∏—á–Ω–∞ –∞–∫—Ç–∏–≤–Ω—ñ—Å—Ç—å:*\n"
                   "‚Ä¢ –ü—Ä–∞—Ü—é–π—Ç–µ –Ω–∞–¥ –ø–æ—Å—Ç–∞–≤–æ—é - –≤–ø–ª–∏–≤–∞—î –Ω–∞ –Ω–µ–π—Ä–æ–µ–Ω–¥–æ–∫—Ä–∏–Ω–Ω—É –ø–µ—Ä–µ–¥–∞—á—É\n"
                   "‚Ä¢ –£–Ω–∏–∫–∞–π—Ç–µ \"—Å–º–∞—Ä—Ç—Ñ–æ–Ω–Ω–æ–≥–æ –≤–∏–≥–∏–Ω—É\" —à–∏—ó\n"
                   "‚Ä¢ –í–∫–ª—é—á—ñ—Ç—å —Å–∏–ª–æ–≤—ñ —Ç—Ä–µ–Ω—É–≤–∞–Ω–Ω—è 3-4 —Ä–∞–∑–∏ –Ω–∞ —Ç–∏–∂–¥–µ–Ω—å\n\n"
                   "3Ô∏è‚É£ *–°—Ç—Ä–µ—Å —ñ –∫–æ—Ä—Ç–∏–∑–æ–ª:*\n"
                   "‚Ä¢ –ê–¥–∞–ø—Ç–æ–≥–µ–Ω–∏: –∞—à–≤–∞–≥–∞–Ω–¥–∞ (30-60 –¥–Ω—ñ–≤), —Ä–æ–¥—ñ–æ–ª–∞ —Ä–æ–∂–µ–≤–∞\n"
                   "‚Ä¢ –ë—ñ–ª–∫–æ–≤–æ-–∂–∏—Ä–æ–≤–∏–π —Å–Ω—ñ–¥–∞–Ω–æ–∫ (—è—î—à–Ω—è, –∞–≤–æ–∫–∞–¥–æ, —Å–∏—Ä)\n"
                   "‚Ä¢ –Ø–∫—ñ—Å–Ω–∏–π —Å–æ–Ω –¥–æ 23:00\n\n"
                   "4Ô∏è‚É£ *–•–∞—Ä—á—É–≤–∞–Ω–Ω—è:*\n"
                   "‚Ä¢ –Ø—î—á–Ω—ñ –∂–æ–≤—Ç–∫–∏, –ø–µ—á—ñ–Ω–∫–∞, —á–µ—Ä–≤–æ–Ω–µ –º'—è—Å–æ, —Ä–∏–±–∞\n"
                   "‚Ä¢ –ù–∞—Å–∏—á–µ–Ω—ñ –∂–∏—Ä–∏, —Ü–∏–Ω–∫, –≤—ñ—Ç–∞–º—ñ–Ω D, –º–∞–≥–Ω—ñ–π, B6, –æ–º–µ–≥–∞-3\n"
                   "‚Ä¢ –î–æ–≤–µ–¥—ñ—Ç—å –≤—ñ—Ç–∞–º—ñ–Ω D –¥–æ 50-70 –Ω–≥/–º–ª\n\n"
                   "5Ô∏è‚É£ *–ë–æ—Ä–æ—Ç—å–±–∞ –∑ –≥–æ—Ä–º–æ–Ω–∞–ª—å–Ω–∏–º–∏ –∞–Ω—Ç–∞–≥–æ–Ω—ñ—Å—Ç–∞–º–∏:*\n"
                   "‚Ä¢ –ó–Ω–∏–∂–µ–Ω–Ω—è –µ—Å—Ç—Ä–æ–≥–µ–Ω—É: B6 (P-5-P), DIM, –∫–∞–ª—å—Ü—ñ–π-D-–≥–ª—é–∫–∞—Ä–∞—Ç\n"
                   "‚Ä¢ –ó–Ω–∏–∂–µ–Ω–Ω—è –ø—Ä–æ–ª–∞–∫—Ç–∏–Ω—É: –≤—ñ—Ç–∞–º—ñ–Ω B6, –º–∞–∫–∞ –ø–µ—Ä—É–∞–Ω—Å—å–∫–∞, –≤—ñ—Ç–µ–∫—Å\n\n"
                   "–î–ª—è —ñ–Ω–¥–∏–≤—ñ–¥—É–∞–ª—å–Ω–æ—ó —ñ–Ω—Ç–µ—Ä–ø—Ä–µ—Ç–∞—Ü—ñ—ó —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤ —Ç–∞ —Ä–æ–∑—Ä–æ–±–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–ª—å–Ω–æ–≥–æ –ø–ª–∞–Ω—É, "
                   "–±—É–¥—å –ª–∞—Å–∫–∞, –∑–≤–µ—Ä–Ω—ñ—Ç—å—Å—è –∑–∞ –∫–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—î—é –¥–æ —Å–ø–µ—Ü—ñ–∞–ª—ñ—Å—Ç–∞: https://t.me/vladyslav_kravchukk"
               )
           else:  # –î–ª—è –∂—ñ–Ω–æ–∫
